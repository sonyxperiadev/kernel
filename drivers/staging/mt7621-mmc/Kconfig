config MTK_MMC
	tristate "MTK SD/MMC"
	depends on !MTD_NAND_RALINK && MMC

config MTK_AEE_KDUMP
	bool "MTK AEE KDUMP"
	depends on MTK_MMC

config MTK_MMC_CD_POLL
	bool "Card Detect with Polling"
	depends on MTK_MMC

config MTK_MMC_EMMC_8BIT
	bool "eMMC 8-bit support"
	depends on MTK_MMC && RALINK_MT7628

