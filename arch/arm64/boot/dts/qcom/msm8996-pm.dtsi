					qcom,pm-cpu-level@1 {  /* C4 */
						reg = <1>;
						label = "fpc-def";
						qcom,spm-cpu-mode = "fpc-def";
						qcom,psci-cpu-mode = <4>;
						qcom,latency-us = <40>;
