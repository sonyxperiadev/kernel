					qcom,pm-cpu-level@0 { /* C1 */
						reg = <0>;
						label = "wfi";
						qcom,psci-cpu-mode = <1>;
						qcom,spm-cpu-mode = "wfi";
						qcom,latency-us = <25>;
						qcom,ss-power = <200>;
						qcom,energy-overhead = <9000>;
						qcom,time-overhead = <60>;
