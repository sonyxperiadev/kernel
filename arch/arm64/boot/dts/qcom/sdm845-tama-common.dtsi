&thermal_zones {
	xo-therm-adc {
		trips {
			active-config0 {
				temperature = <2147483647>;
				hysteresis = <(-1)>;
			};
		};
	};

	msm-therm-adc {
		trips {
			active-config0 {
				temperature = <2147483647>;
				hysteresis = <(-1)>;
			};
		};
	};

	ufs_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm8998_adc_tm 0x4e>;
