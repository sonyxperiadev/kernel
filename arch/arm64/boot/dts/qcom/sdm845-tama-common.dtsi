			    &pm8998_gpio_11 &pm8998_gpio_12 &pm8998_gpio_13 &pm8998_gpio_14
			    &pm8998_gpio_16 &pm8998_gpio_17 &pm8998_gpio_18 &pm8998_gpio_19
			    &pm8998_gpio_21
