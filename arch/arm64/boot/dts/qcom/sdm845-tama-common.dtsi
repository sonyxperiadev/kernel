		compatible = "somc,water-detection";
		dvdt_enable_gpio = <&tlmm 28 0>;
		dvdt_wrt_det_and = <&tlmm 29 0>;
		dvdt_wrt_det_or  = <&tlmm 30 0>;
	};

	/* I2C: QUP5 */
	i2c@894000 {
		pinctrl-0 = <&sdm_gpio_85 &sdm_gpio_86>;
		pinctrl-1 = <&sdm_gpio_85_suspend &sdm_gpio_86_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* UART: QUP6 */
	qcom,qup_uart@898000 {
		clock-frequency = <4000000>;
		status = "okay";
		qcom,auto-suspend-disable = <1>;
	};

	/* SPI: QUP8 */
	spi@a80000 {
		status = "okay";
	};

	/* UART: QUP9 */
	qcom,qup_uart@a84000 {
		pinctrl-0 = <&sdm_gpio_4 &sdm_gpio_5>;
		pinctrl-1 = <&sdm_gpio_4 &sdm_gpio_5>;
		status = "okay";
	};

	/* I2C: QUP10 */
	i2c@a88000 {
		pinctrl-0 = <&sdm_gpio_55 &sdm_gpio_56>;
		pinctrl-1 = <&sdm_gpio_55_suspend &sdm_gpio_56_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* I2C: QUP14 */
	i2c@a98000 {
		pinctrl-0 = <&sdm_gpio_33 &sdm_gpio_34>;
		pinctrl-1 = <&sdm_gpio_33_suspend &sdm_gpio_34_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
		tcs3490@72 {
			compatible = "ams,tcs3490";
			reg = <0x72>;
			interrupt-parent = <&tlmm>;
			interrupts = <11 0x0>;
			rgbcir_vdd-supply = <&pm8998_l22>;
			rgbcir_vio-supply = <&cam_vio_vreg>;
			pinctrl-names = "rgbcir_irq_active", "rgbcir_irq_suspend";
			pinctrl-0 = <&sdm_gpio_11>;
			pinctrl-1 = <&sdm_gpio_11_suspend>;
			ams,rgbcir-vdd-supply = <1>;
			ams,rgbcir-gpio-vdd = <0>;
			ams,rgbcir-vio-supply = <1>;
		};
		tof_sensor@52 {
			compatible = "tof_sensor";
			reg = <0x52>;
			interrupt-parent = <&tlmm>;
			interrupts = <22 0x0>;
			tof_avdd-supply = <&pm8998_l22>;
			tof_vio-supply = <&cam_vio_vreg>;
			pinctrl-names = "tof_irq_active", "tof_irq_suspend";
			pinctrl-0 = <&sdm_gpio_22 &sdm_gpio_23>;
			pinctrl-1 = <&sdm_gpio_22_suspend &sdm_gpio_23>;
			tof-reset-gpio = <&tlmm 23 0>;
			sony,tof-sensor-name = "VL53L0";
			sony,tof-need-cam-on = <1>;
			sony,tof-avdd-supply = <1>;
			sony,tof-gpio-avdd = <0>;
			sony,tof-vio-supply = <1>;
			sony,tof-sensor-facing = <0>;
		};
	};

	cam_vio_vreg: cam_vio_vreg {
		compatible = "regulator-fixed";
		regulator-name = "camera_vio_vreg";
		startup-delay-us = <0>;
