&qupv3_se3_i2c {
	status = "disabled";
};

&qupv3_se5_i2c {
	status = "okay";

	synaptics_clearpad: synaptics_clearpad@2c {
		compatible = "synaptics,clearpad";
		reg = <0x2c>;
		interrupt-parent = <&tlmm>;
		interrupts = <125 0x2008>;
		synaptics,irq_gpio = <&tlmm 125 0x2008>;
		post_probe_start = <1>;
		synaptics,firmware_name = "touch_module_id_0x%02x.img";
		flash_on_post_probe = <0>;
		flip_config = <0>;
		watchdog_enabled = <1>;
		watchdog_delay_ms = <10000>;
		charger_supported = <0>;
		pen_supported = <0>;
		glove_supported = <1>;
		cover_supported = <1>;
		touch_pressure_enabled = <1>;
		touch_size_enabled = <0>;
		touch_orientation_enabled = <0>;
		preset_x_max = <1079>;
		preset_y_max = <2159>;
		preset_n_fingers = <10>;
		wakeup_gesture {
			double_tap {
				gesture_code = <0x0003>;
				event_00 {
					type = <2>; /* LOG */
					message = "=== DOUBLE TAP ===";
				};
				event_01 {
					type = <1>; /* KEY */
					code = <531>; /* TOUCHPAD_ON */
					down = <1>;
				};
				event_02 {
					type = <1>; /* KEY */
					code = <531>; /* TOUCHPAD_ON */
					down = <0>;
				};
				event_03 {
					type = <99>; /* END */
				};
			};
		};

		/* chip settings */
		clearpad_default {
			flash_default_timeout_ms = <20000>;
			calibrate_on_fwflash = <0>;
			calibration_supported = <0>;
			hwreset_delay_for_powerup_ms = <220>;
			interrupt_default_wait_ms = <1000>;
			charger_only_delay_ms = <200>;
		};
		S3330 {
			flash_default_timeout_ms = <20000>;
			calibrate_on_fwflash = <0>;
			calibration_supported = <0>;
			hwreset_delay_for_powerup_ms = <220>;
			interrupt_default_wait_ms = <1000>;
			charger_only_delay_ms = <200>;
		};
		S332U {
			flash_default_timeout_ms = <20000>;
			calibrate_on_fwflash = <1>;
			calibration_supported = <1>;
			hwreset_delay_for_powerup_ms = <220>;
			interrupt_default_wait_ms = <1000>;
			charger_only_delay_ms = <200>;
		};
		TD4353jdi {
			flash_default_timeout_ms = <20000>;
			calibrate_on_fwflash = <0>;
			calibration_supported = <0>;
			hwreset_delay_for_powerup_ms = <220>;
			interrupt_default_wait_ms = <1000>;
			charger_only_delay_ms = <200>;
		};
	};

	synaptics_tcm: synaptics_tcm@3c {
		compatible = "synaptics,tcm-i2c";
		reg = <0x3c>;
		interrupt-parent = <&tlmm>;
		interrupts = <125 0x2008>;
		vdd-supply = <&pm8998_l14>;
		pinctrl-names = "pmx_ts_active", "pmx_ts_suspend";
		pinctrl-0 = <&sde_dsi_active &sde_te_active>;
		pinctrl-1 = <&sde_dsi_suspend &sde_te_suspend>;
		synaptics,irq-gpio = <&tlmm 125 0x2008>; /* IRQF_ONESHOT | IRQF_TRIGGER_LOW */
		synaptics,irq-on-state = <0>;
		synaptics,reset-on-state = <0>;
		synaptics,reset-active-ms = <20>;
		synaptics,reset-delay-ms = <200>;
		synaptics,power-delay-ms = <200>;
		synaptics,ubl-i2c-addr = <0x2c>;
	};
};

&qupv3_se10_i2c {
	nfc@28 {
		compatible = "nxp,pn547";
		reg = <0x28>;
		interrupt-parent = <&tlmm>;
		interrupts = <63 0x1>;
		nxp,irq_gpio = <&tlmm 63 0x00>;
		nxp,dwld_en = <&tlmm 62 0x00>;
		nxp,ven = <&tlmm 12 0x01>;
		qcom,clk-src = "BBCLK3";
		qcom,clk-gpio = <&pm8998_gpios 21 0x00>;
		clock-names = "nfc_clk";
		clocks = <&clock_rpmh RPMH_LN_BB_CLK3>;
		pinctrl-names = "pn547-active", "pn547-inactive";
		pinctrl-0 = <&nfc_int_active &nfc_enable_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_enable_suspend>;
	};
};


&spmi_bus {
	qcom,pm8998@0 {
		/* Reset Configuration */
		qcom,power-on@800 {
			qcom,s3-src = "resin";
			qcom,pon_1 {
				/delete-property/ qcom,support-reset;
				/delete-property/ qcom,s1-timer;
				/delete-property/ qcom,s2-timer;
				/delete-property/ qcom,s2-type;
			};
			qcom,pon_2 {
				linux,code = <115>;
			};
			qcom,pon_3 {
				/delete-property/ qcom,support-reset;
				/delete-property/ qcom,s1-timer;
				/delete-property/ qcom,s2-timer;
				/delete-property/ qcom,s2-type;
			};
		};
	};
};

