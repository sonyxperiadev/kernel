			     &sm_gpio_64 &sm_gpio_65 &sm_gpio_66 &sm_gpio_67 &sm_gpio_68
			     &sm_gpio_69 &sm_gpio_70 &sm_gpio_71 &sm_gpio_72 &sm_gpio_73
			     &sm_gpio_74 &sm_gpio_75 &sm_gpio_83 &sm_gpio_84 &sm_gpio_85
			     /*&sm_gpio_86*/ &sm_gpio_87 &sm_gpio_88 &sm_gpio_89 &sm_gpio_90
			     &sm_gpio_92 &sm_gpio_93 &sm_gpio_97 &sm_gpio_98 &sm_gpio_99
			     &sm_gpio_100 &sm_gpio_101 &sm_gpio_102 &sm_gpio_103 &sm_gpio_104
			     &sm_gpio_105 &sm_gpio_106 &sm_gpio_107 &sm_gpio_108 &sm_gpio_109
			     &sm_gpio_110 &sm_gpio_111 &sm_gpio_112 &sm_gpio_113 &sm_gpio_114
			     &sm_gpio_115 &sm_gpio_116 &sm_gpio_117 &sm_gpio_118 &sm_gpio_119
