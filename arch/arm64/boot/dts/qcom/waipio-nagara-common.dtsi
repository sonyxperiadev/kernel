		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

        bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 13 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	egistec_fp {
		status = "ok";
		compatible = "fp-egistec";
		egistec,gpio_reset = <&tlmm 42 0x00>; 		// reset gpio pin num
		egistec,gpio_irq = <&tlmm 40 0x00>; 		// irq gpio pin num
		egistec,gpio_vcc_en= <&pm8450_gpios 1 0x00>; 		// vcc enable gpio pin num
		fp-gpio-vcc-enable; 						// use gpio enable power or not
		fp-gpio-ext-ldo;
		pinctrl-names = "egis_rst_active", "egis_rst_sleep", "egis_vcc_high", "egis_vcc_low", "egis_irq_active", "egis_irq_low";
		pinctrl-0 = <&EGIS_RST_active>;
		pinctrl-1 = <&EGIS_RST_sleep>;
		pinctrl-2 = <&EGIS_VCC_active>;
		pinctrl-3 = <&EGIS_VCC_sleep>;
		pinctrl-4 = <&sm_gpio_40_input_no_pull>;
		pinctrl-5 = <&sm_gpio_40>;
		egis-vcc-supply = <&pm8350c_bob>;
		egis-fp,vcc-voltage = <3350000 3800000>;
		egis-fp,vcc-current = <1000000>;
		egis-fp,vcc-max-voltage = <3960000>;
	};

	last_logs@ffcc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};
};

&tlmm {
	EGIS_RST_active: EGIS_RST_active {
		mux {
			pins = "gpio42";
			function = "gpio";
		};

		config {
			pins = "gpio42";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	EGIS_RST_sleep: EGIS_RST_sleep {
		mux {
			pins = "gpio42";
			function = "gpio";
		};

		config {
			pins = "gpio42";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
};

&pm8450_gpios {
	EGIS_VCC_active: EGIS_VCC_active {
		pins = "gpio1";
		function = "normal";
		output-high;
		bias-disable;
		power-source = <1>;
	};

	EGIS_VCC_sleep: EGIS_VCC_sleep {
		pins = "gpio1";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <1>;
	};
};

