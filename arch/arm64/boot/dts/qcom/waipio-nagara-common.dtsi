			     &sm_gpio_32 &sm_gpio_33 &sm_gpio_34 &sm_gpio_35 &sm_gpio_36
			     &sm_gpio_37 &sm_gpio_38 &sm_gpio_39 &sm_gpio_40 &sm_gpio_41
			     &sm_gpio_42 &sm_gpio_44 &sm_gpio_45 &sm_gpio_46 &sm_gpio_47
