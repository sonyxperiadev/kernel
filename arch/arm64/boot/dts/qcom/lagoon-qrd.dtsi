&qupv3_se8_i2c {
	status = "okay";
	qcom,i2c-touch-active="sec,sec_ts";

	sec_ts@48 {
		compatible = "sec,sec_ts";
		reg = <0x48>;
		interrupt-parent = <&tlmm>;
		interrupts = <22 0x2008>;
		pinctrl-names = "pmx_ts_active","pmx_ts_suspend",
					"pmx_ts_release";
		pinctrl-0 = <&ts_active>;
		pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
		pinctrl-2 = <&pmx_ts_release>;
		vdd-supply = <&L11A>;
		avdd-supply = <&L6A>;
		sec,irq-gpio = <&tlmm 22 0x2008>;
		sec,irq_type = <8200>;
		sec,reset-gpio = <&tlmm 21 0x00>;
		sec,avdd-en-gpio = <&tlmm 10 0x00>;
		sec,mis_cal_check = <1>;
		sec,regulator_dvdd = "vdd";
		sec,regulator_avdd = "avdd";
		sec,regulator_boot_on;
		sec,i2c-burstmax = <32>;
		sec,project_name = "PJT", "MODEL";
		sec,max_coords = <1080 2520>;
		sec,always_lpmode = <0>;
		sec,bringup = <0>;
		sec,firmware_name = "s6sy761_p213.bin";

		panel = <&dsi_pdx213_amoled_cmd>;

		/* feature */
                sec,rejection_area_portrait = <25 150 60 200>;
                sec,rejection_area_landscape = <150 20 200 60>;
	};
};

&pm7250b_vadc {
	smb1390_therm@e {
		qcom,scale-fn-type = <ADC_SCALE_HW_CALIB_PM5_SMB1398_TEMP>;
	};
};

&pm7250b_gpios {
	smb_stat {
		smb_stat_default: smb_stat_default {
			pins = "gpio6";
			function = "normal";
			input-enable;
			bias-pull-up;
			qcom,pull-up-strength = <PMIC_GPIO_PULL_UP_30>;
			power-source = <0>;
		};
	};
};

&qupv3_se10_i2c {
	qcom,clk-freq-out = <400000>;
	status = "ok";

	aw882xx_smartpa@34 {
		compatible = "awinic,aw882xx_smartpa";
		reg = <0x34>;
		reset-gpio = <&tlmm 86 0>;
		irq-gpio = <&tlmm 60 0x2008>;
		monitor-flag = <1>;
		monitor-timer-val = <3000>;
		status = "ok";
		aw-cali-mode = "aw_misc";
	};

	fs16xx@35 {
		compatible = "foursemi,fs16xx";
		reg = <0x35>;
		status = "okay";
		fsm,rst-gpio = <&tlmm 86 0>;
	};
	#include "smb1398.dtsi"
};

