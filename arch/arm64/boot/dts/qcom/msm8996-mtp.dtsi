	pinctrl-0 = <&pm8994_gpio_1 &pm8994_gpio_2 &pm8994_gpio_3
		     &pm8994_gpio_4 &pm8994_gpio_5 &pm8994_gpio_6
		     &pm8994_gpio_7 &pm8994_gpio_8 &pm8994_gpio_9
		     &pm8994_gpio_10 &pm8994_gpio_11 &pm8994_gpio_11
		     &pm8994_gpio_13 &pm8994_gpio_14 &pm8994_gpio_15
		     &pm8994_gpio_16 &pm8994_gpio_17 &pm8994_gpio_18
		     &pm8994_gpio_19>;

	pm8994_gp1 {
		pm8994_gpio_1: gpio1 {
			pins = "gpio1";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp2 {
		pm8994_gpio_2: gpio2 {
			pins = "gpio2";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp3 {
		pm8994_gpio_3: gpio3 {
			pins = "gpio3";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp4 {
		pm8994_gpio_4: gpio4 {
			pins = "gpio4";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp5 {
		pm8994_gpio_5: gpio5 {
			pins = "gpio5";
			function = "normal";
			//input-enable;
			drive-push-pull;
			bias-high-impedance;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	pm8994_gp6 {
		pm8994_gpio_6: gpio6 {
			pins = "gpio6";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 7 - NFC DWL REQ */
	nfc_dwl_req {
		pm8994_gpio_7: nfc_download {
			pins = "gpio7";
			function = "normal";
			output-low;
			drive-push-pull;
			//qcom,pull-up-strength = <PMIC_GPIO_PULL_UP_NO>;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_HIGH>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 8 - WLAN_EN - CMOS GPIO Output, PD10, SRC VIN2 */
	wlan_en {
		pm8994_gpio_8: wlan_enable {
			pins = "gpio8";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-pull-down;
			//needed? qcom,pull-up-strength = <PMIC_GPIO_PULL_DN_10>;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 9 - Rome 3.3V control - CMOS GPIO Output HIGH, SRC VPH_PWR */
	lan_en {
		pm8994_gpio_9: wwlan_enable {
			pins = "gpio9";
			function = "normal";
			output-high;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
			power-source = <PM8994_VPP_VPH>;
		};
	};

	/* GPIO 10 - NFC CLK _REQ */
	nfc_clk_req {
		pm8994_gpio_10: nfc_clock {
			pins = "gpio10";
			function = "normal";
			input-enable;
			drive-push-pull;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp11 {
		pm8994_gpio_11: gpio11 {
			pins = "gpio11";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp12 {
		pm8994_gpio_12: gpio12 {
			pins = "gpio12";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 13 - HPH_EN0 - CMOS GPIO Output HIGH, SRC1.8V */
	hph_en0 {
		pm8994_gpio_13: hph0_enable {
			pins = "gpio13";
			function = "normal";
			output-high;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 14 - lcd_bklt_reg_en - CMOS GPIO Output HIGH, SRC1.8V */
	lcd_bklt_reg_en {
		pm8994_gpio_14: lcd_backlight {
			pins = "gpio14";
			function = "normal";
			output-high;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp15 {
		pm8994_gpio_15: gpio15 {
			pins = "gpio15";
			function = "func1";
			output-low;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp16 {
		pm8994_gpio_16: gpio16 {
			pins = "gpio16";
			function = "normal";
			drive-push-pull;
		};
	};

	pm8994_gp17 {
		pm8994_gpio_17: gpio17 {
			pins = "gpio17";
			function = "normal";
			drive-push-pull;
		};
	};

	/* GPIO 18 - Rome Sleep Clock - CMOS GPIO Output LOW, F2, SRC1.8V */
	rome_sleep_clk {
		pm8994_gpio_18: rome_sleep {
			pins = "gpio18";
			function = "func2";
			output-low;
			drive-push-pull;
			bias-disable;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_MED>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	/* GPIO 19 - Rome BT Reset - CMOS GPIO Output LOW, F2, SRC1.8V */
	pm8994_gp19 {
		pm8994_gpio_19: gpio19 {
			pins = "gpio19";
			function = "normal";
			output-low;
			drive-push-pull;
			bias-pull-down;
			qcom,drive-strength = <PMIC_GPIO_STRENGTH_NO>;
			power-source = <PM8994_VPP_1P8>;
		};
	};

	pm8994_gp22 {
		pm8994_gpio_22: gpio22 {
			pins = "gpio17";
			function = "normal";
			drive-push-pull;
		};
	};
};

&pmi8994_haptics {
	status = "okay";
};

&flash_led {
	qcom,follow-otst2-rb-disabled;
};

&blsp1_uart2 {
	status = "ok";
};

&i2c_6 {
	at24@51 {
		compatible = "atmel,24c32";
		reg = <0x51>;
	};
};

&i2c_7 {
	silabs4705@11 { /* SiLabs FM chip, slave id 0x11*/
		status = "ok";
		compatible = "silabs,si4705";
		reg = <0x11>;
		vdd-supply = <&pm8994_s4>;
		silabs,vdd-supply-voltage = <1800000 1800000>;
		va-supply = <&rome_vreg>;
		silabs,va-supply-voltage = <3300000 3300000>;
		pinctrl-names = "pmx_fm_active","pmx_fm_suspend";
		pinctrl-0 = <&fm_int_active &fm_status_int_active &fm_rst_active>;
		pinctrl-1 = <&fm_int_suspend &fm_status_int_suspend &fm_rst_suspend>;
		silabs,reset-gpio = <&tlmm 39 0>;
		silabs,int-gpio = <&tlmm 38 0>;
		silabs,status-gpio = <&tlmm 78 0>;
		#address-cells = <0>;
		interrupts = <0 1>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0xffffffff>;
		interrupt-map = <
				  0 &tlmm 38 2
				  1 &tlmm 78 1
				>;
		interrupt-names = "silabs_fm_int", "silabs_fm_status_int";
	};
};

&i2c_8 { /* BLSP2 QUP2 */
	nq@28 {
		compatible = "qcom,nq-nci";
		reg = <0x28>;
		qcom,nq-irq = <&tlmm 9 0x00>;
		qcom,nq-ven = <&tlmm 12 0x00>;
		qcom,nq-firm = <&pm8994_gpios 7 0x00>;
		qcom,nq-clkreq = <&pm8994_gpios 10 0x00>;
		interrupt-parent = <&tlmm>;
		qcom,clk-src = "BBCLK2";
		interrupts = <9 0>;
		interrupt-names = "nfc_irq";
		pinctrl-names = "nfc_active", "nfc_suspend";
		pinctrl-0 = <&nfc_int_active &nfc_disable_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_disable_suspend>;
		clocks = <&clock_rpmcc RPM_SMD_BB_CLK2_PIN>;
		clock-names = "ref_clk";
	};
};

&wil6210 {
	status = "ok";
};
