#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/interrupt-controller/irq.h>

#include "pm6125.dtsi"
#include "pmr735a.dtsi"

#define PMK8350_SID	6

#include "pmk8350.dtsi"

&pmr735a_gpios {
	key_vol_up {
		key_vol_up_default: key_vol_up_default {
			pins = "gpio1";
			function = "normal";
			input-enable;
			bias-pull-up;
			power-source = <1>;
		};
	};
	pmr735a_charger_skin_therm {
	pmr735a_charger_skin_therm_default: pmr735a_charger_skin_therm_default {
			pins = "gpio3";
			bias-high-impedance;
		};
	};
};

&spmi_bus {
	qcom,pmr735a@4 {
		/* Example configuration to enable PMR735A VADC in standalone mode */
		pmr735a_vadc: vadc@3600 {
			compatible = "qcom,spmi-adc7-sw-calib";
			reg = <0x3600>, <0x3900>;
			reg-names = "adc5-sw-calib-pbs-base", "adc5-sw-calib-cmn-base";
			#address-cells = <1>;
			#size-cells = <0>;
			interrupts = <0x4 0x36 0x0 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "eoc";
			#io-channel-cells = <1>;
			io-channel-ranges;
			status = "ok";

			/* PMR735a Channel nodes */
			pmr735a_ref_gnd {
				reg = <ADC7_REF_GND>;
				label = "pmr735a_ref_gnd";
				qcom,pre-scaling = <1 1>;
				qcom,no-cal;
			};

			pmr735a_vref_1p25 {
				reg = <ADC7_1P25VREF>;
				label = "pmr735a_vref_1p25";
				qcom,pre-scaling = <1 1>;
				qcom,no-cal;
			};

			pmr735a_vref_vdd {
				reg = <ADC7_VREF_VADC>;
				label = "pmr735a_vref_vdd";
				qcom,pre-scaling = <1 1>;
				qcom,no-cal;
			};

			pmr735a_die_temp {
				reg = <ADC7_DIE_TEMP>;
				label = "pmr735a_die_temp";
				qcom,pre-scaling = <1 1>;
			};
			pmr735a_charger_skin_therm {
				reg = <ADC7_GPIO3_100K_PU>;
				label = "pmr735a_charger_skin_therm";
				qcom,ratiometric;
				qcom,hw-settle-time = <200>;
				qcom,pre-scaling = <1 1>;
				bias-high-impedance;
			};
		};
	};
};

&pm6125_vadc {
	pinctrl-names = "default";
	pinctrl-0 = <
			&pa_therm2_default
			&msm_therm_default
			&pm6125_batt_therm_default
			&get_stageid_adc_mb_default>;

	/delete-node/ xo_therm;

	pa_therm1 {
		reg = <ADC5_AMUX_THM1_100K_PU>;
		label = "pa_therm1";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	quiet_therm {
		reg = <ADC5_AMUX_THM2_100K_PU>;
		label = "quiet_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm2 {
		reg = <ADC5_GPIO4_100K_PU>;
		label = "pa_therm2";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	msm_therm {
		reg = <ADC5_GPIO1_100K_PU>;
		label = "msm_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pm6125_batt_therm {
		reg = <ADC5_GPIO2_100K_PU>;
		label = "pm6125_batt_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
