			skin3_cpu2 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU2 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin3_cpu3 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU3 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin3_cpu4 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU4 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-16)>;
			};
			skin3_cpu5 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU5 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-16)>;
			};
			skin3_cpu6 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU6 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-16)>;
			};
			skin3_cpu7 {
				trip = <&cpu_ct_trip3>;
				cooling-device = <&CPU7 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-16)>;
			};


			/* Throttle from FMAX to 1824-1574.4MHz */
			skin4_cpu0 {
				trip = <&cpu_ct_trip4>;
				cooling-device = <&CPU0 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin4_cpu1 {
				trip = <&cpu_ct_trip4>;
				cooling-device = <&CPU1 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin4_cpu2 {
				trip = <&cpu_ct_trip4>;
				cooling-device = <&CPU2 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin4_cpu3 {
				trip = <&cpu_ct_trip4>;
				cooling-device = <&CPU3 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-21)>;
			};
			skin4_cpu4 {
				trip = <&cpu_ct_trip4>;
				cooling-device = <&CPU4 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-18)>;
			};
			skin4_cpu5 {
				trip = <&cpu_ct_trip4>;
