			};
			skin5_cpu6 {
				trip = <&cpu_ct_trip5>;
				cooling-device = <&CPU6 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-24)>;
			};
			skin5_cpu7 {
				trip = <&cpu_ct_trip5>;
				cooling-device = <&CPU7 THERMAL_NO_LIMIT
							(THERMAL_MAX_LIMIT-24)>;
			};

			/* Throttle C1 only from FMAX to 2361.6MHz */
