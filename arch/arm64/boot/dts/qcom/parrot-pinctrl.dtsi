&soc {
	tlmm: pinctrl@f000000 {
		compatible = "qcom,parrot-pinctrl";
		reg = <0xf000000 0x1000000>;
		interrupts = <GIC_SPI 208 IRQ_TYPE_LEVEL_HIGH>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		wakeup-parent = <&pdc>;
		qcom,gpios-reserved = <0 1 2 3 38>;


		bt_en_sleep: bt_en_sleep {
				mux {
						pins = "gpio35";
						function = "gpio";
				};

				config {
						pins = "gpio35";
						drive-strength = <2>;
						output-low;
						bias-pull-down;
				};
		};

