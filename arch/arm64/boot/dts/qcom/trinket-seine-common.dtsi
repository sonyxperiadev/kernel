	/* GPIO_6 : TS_I2C_SDA */
	sm_gpio_6: sm_gpio_6 {
		mux {
			pins = "gpio6";
			function = "qup02";
		};

		config {
			pins = "gpio6";
			drive-strength = <2>;
			bias-disable;
		};
	};

