&spmi_bus {
	qcom,pm6125@0 {
		qcom,power-on@800 {
			qcom,pon_2 {
				linux,code = <KEY_VOLUMEUP>;
			};
		};
	};
};

&soc {
	gpio_keys {
		pinctrl-0 = <&sm_gpio_47>;
		vol_dn {
			label = "volume_down";
			gpios = <&tlmm 47 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		vol_up {
			status = "disabled";
		};
	};

	somc_water_detection {
		compatible = "somc,water-detection";
		dvdt_enable_gpio = <&tlmm 129 0>;
		dvdt_wrt_det_and = <&tlmm 123 0>;
		dvdt_wrt_det_or = <&tlmm 124 0>;
	};
};

&sdhc_2 {
	cd-gpios = <&tlmm 98 GPIO_ACTIVE_HIGH>;
	uim2-gpios = <&tlmm 101 GPIO_ACTIVE_HIGH>;

	qcom,clk-rates = <400000 20000000 25000000 40000000 80000000>;
	qcom,bus-speed-mode = "DDR50";
};

&pmi632_vadc {
	bat_therm {
		qcom,lut-index = <0>;
	};
};

&thermal_zones {
	soc {
		trips {
			low-soc {
				temperature = <5>;
				hysteresis = <0>;
				type = "passive";
			};
		};
	};
};

&pmi632_rgb {
	somc,rgb_sync = <1>;
	red {
		linux,default-trigger = "none";
		somc,color_variation_max_num = <4>;
		somc,max_current = <
			103 50 36
			104 50 36
			105 50 36
			106 50 36>;
	};
	green {
		linux,default-trigger = "none";
		somc,color_variation_max_num = <4>;
		somc,max_current = <
			103 21 48
			104 21 48
			105 21 48
			106 21 48>;
	};
	blue {
		linux,default-trigger = "none";
		somc,color_variation_max_num = <4>;
		somc,max_current = <
			103 98 25
			104 98 25
			105 98 25
			106 98 25>;
	};
};

&pmi632_pwm {
	qcom,sync-channel-ids = <1 2 3>;
};

