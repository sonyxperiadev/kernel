#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/*
 *     Seine platform: debug serial at qup_uart@4a90000
 * Define SONY_SEINE_DEBUG_UART in DT to enable the UART output:
 * for the regular non-debugging case, it is kept disabled in
 * order to save power and decrease boot times.
 */

/ {
	aliases {
		serial0 = &qupv3_se4_2uart;
	};
};

&reserved_memory {
	debug_region: debug_region@ffb00000 {
		compatible = "removed-dma-pool", "qcom,debug_memory";
		no-map;
		reg = <0 0xffb00000 0 0xc0000>;
		label = "debug_mem";
	};

	last_log_region: last_log_region@ffbc0000 {
		compatible = "removed-dma-pool", "qcom,last_log_memory";
		no-map;
		reg = <0 0xffbc0000 0 0x80000>;
		label = "last_log";
		phandle = "log";
	};

	ramoops: ramoops@ffc00000 {
		compatible = "removed-dma-pool", "ramoops";
		no-map;
		reg = <0 0xffc40000 0 0x000c0000>;
		record-size = <0x1000>;
		console-size = <0x40000>;
		ftrace-size = <0x0>;
		pmsg-size = <0x20000 0x20000>;
		ecc-size = <16>;
	};

	cmdline_region: cmdline_region@ffd00000 {
		compatible = "removed-dma-pool", "cmdline_region";
		no-map;
		reg = <0 0xffd40000 0 0x1000>;
		label = "cmdline_region";
	};
};

&soc {
	qupv3_se4_2uart: qcom,qup_uart@4a90000 {
		compatible = "qcom,msm-geni-console";
		reg = <0x4a90000 0x4000>;
		reg-names = "se_phys";
		clock-names = "se-clk", "m-ahb", "s-ahb";
		clocks = <&clock_gcc GCC_QUPV3_WRAP0_S4_CLK>,
			<&clock_gcc GCC_QUPV3_WRAP_0_M_AHB_CLK>,
			<&clock_gcc GCC_QUPV3_WRAP_0_S_AHB_CLK>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&qupv3_se4_2uart_active>;
		pinctrl-1 = <&qupv3_se4_2uart_sleep>;
		interrupts = <GIC_SPI 331 0>;
		qcom,wrapper-core = <&qupv3_0>;
	    #ifdef SONY_SEINE_DEBUG_UART
		status = "ok";
	    #else
		status = "disabled";
	    #endif
	};

	restart-reason {
		none {
			cmd = "none";
			reg-val = <0x00 0x00000000>;
		};

		unknown {
			cmd = "unknown";
			reg-val = <0x01 0x77665501>;
		};

		recovery {
			cmd = "recovery";
			reg-val = <0x02 0x77665500>;
			reboot-cmd;
		};

		bootloader {
			cmd = "bootloader";
			reg-val = <0x03 0x77665500>;
			reboot-cmd;
		};

		rtc {
			cmd = "rtc";
			reg-val = <0x04 0x77665500>;
			reboot-cmd;
		};

		dm-verity-device-corrupted {
			cmd = "dm-verity device corrupted";
			reg-val = <0x05 0x77665508>;
			reboot-cmd;
		};

		dm-verity-enforcing {
			cmd = "dm-verity enforcing";
			reg-val = <0x06 0x77665509>;
			reboot-cmd;
		};

		keys-clear {
			cmd = "keys clear";
			reg-val = <0x07 0x7766550A>;
			reboot-cmd;
		};

		panic {
			cmd = "panic";
			reg-val = <0x40 0xC0DEDEAD>;
		};

		oem-N {
			cmd = "oem-4E";
			reg-val = <0x61 0x6F656D4E>;
			reboot-cmd;
		};

		oem-S {
			cmd = "oem-53";
			reg-val = <0x60 0x6F656D53>;
			reboot-cmd;
		};

		oem-P {
			cmd = "oem-50";
			reg-val = <0x51 0x6F656D50>;
			reboot-cmd;
		};
	};

	last_logs@ffbc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};

	nfc,pm-ops {
		compatible = "sony,pn553-pm-ops";
	};

	pinctrl@400000 {
		disabled-pins = <22 23 28 29 30 31 32 33>;
	};

	somc_pinctrl: somc_pinctrl@0 {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = <&sm_gpio_0 &sm_gpio_1 &sm_gpio_2 &sm_gpio_3 &sm_gpio_4
			     &sm_gpio_5 &sm_gpio_6 &sm_gpio_7 &sm_gpio_8 &sm_gpio_9
			     &sm_gpio_14 &sm_gpio_15 &sm_gpio_18 &sm_gpio_19 &sm_gpio_20
			     &sm_gpio_21 &sm_gpio_24 &sm_gpio_25 &sm_gpio_26 &sm_gpio_27
			     &sm_gpio_34 &sm_gpio_35 &sm_gpio_36 &sm_gpio_37 &sm_gpio_38
			     &sm_gpio_39 &sm_gpio_40 &sm_gpio_41 &sm_gpio_42 &sm_gpio_44
			     &sm_gpio_46 &sm_gpio_47 /*&sm_gpio_48*/ &sm_gpio_49 &sm_gpio_52
			     &sm_gpio_53 /*&sm_gpio_54*/ &sm_gpio_55 &sm_gpio_57 /*&sm_gpio_58*/
			     &sm_gpio_75 &sm_gpio_79 &sm_gpio_80 &sm_gpio_81 &sm_gpio_82
			     &sm_gpio_83 &sm_gpio_84 &sm_gpio_85 &sm_gpio_86 &sm_gpio_87
			     /*&sm_gpio_88*/ &sm_gpio_89 /*&sm_gpio_90*/ &sm_gpio_91 &sm_gpio_92
			     &sm_gpio_93 &sm_gpio_94 &sm_gpio_95 /*&sm_gpio_97*/ &sm_gpio_98
			     &sm_gpio_99 &sm_gpio_100 &sm_gpio_101 &sm_gpio_104 &sm_gpio_113
			     &sm_gpio_114 &sm_gpio_115 &sm_gpio_116 &sm_gpio_117 &sm_gpio_118
			     &sm_gpio_119 &sm_gpio_123 &sm_gpio_124 &sm_gpio_125 &sm_gpio_127
			     &sm_gpio_129 &sm_gpio_130 &sm_gpio_131>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	tlmm_gpio_100_reg: tlmm_gpio_100_reg {
		compatible = "regulator-fixed";
		regulator-name = "tlmm_gpio_100_reg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&tlmm 100 0>;
	};

	et6xx {
		compatible = "etspi,et510";
		interrupt-parent = <&tlmm>;
		interrupts = <92 0x0>;
		et51x,gpio_irq = <&tlmm 92 0>;
		vdd_ana-supply = <&tlmm_gpio_100_reg>;
		et51x,no-low-voltage-probe;

		pinctrl-names = "et51x_reset_reset",
				"et51x_reset_active",
				"et51x_irq_active";
		pinctrl-0 = <&sm_gpio_93>;
		pinctrl-1 = <&sm_gpio_93_output_high>;
		pinctrl-2 = <&sm_gpio_92>;
	};

	somc_pinctrl_pmic: somc_pinctrl@1 {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pm6125_gpio_1 &pm6125_gpio_2 &pm6125_gpio_3 &pm6125_gpio_5
			     &pm6125_gpio_6 &pm6125_gpio_7 &pm6125_gpio_8 &pm6125_gpio_9
			     &pmi632_gpio_1 &pmi632_gpio_2 &pmi632_gpio_3 &pmi632_gpio_4
			     &pmi632_gpio_5 &pmi632_gpio_6 &pmi632_gpio_7 &pmi632_gpio_8>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 94 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	cdc_pri_mi2s_gpios: msm_cdc_pinctrl_pri {
		compatible = "qcom,msm-cdc-pinctrl";

		pinctrl-names = "aud_active", "aud_sleep";
		pinctrl-0 = <&pri_i2s_sck_active &pri_i2s_ws_active
			    &pri_i2s_data0_active &pri_i2s_data1_active>;
		pinctrl-1 = <&pri_i2s_sck_sleep &pri_i2s_ws_sleep
			    &pri_i2s_data0_sleep &pri_i2s_data1_sleep>;
	};

	pri_dmic_gpios: pri_dmic_pinctrl {
		compatible = "qcom,msm-cdc-pinctrl";
		pinctrl-names = "aud_active", "aud_sleep";
		pinctrl-0 = <&cdc_dmic01_clk_active &cdc_dmic01_data_active>;
		pinctrl-1 = <&cdc_dmic01_clk_sleep &cdc_dmic01_data_sleep>;
	};

	sec_dmic_gpios: sec_dmic_pinctrl {
		compatible = "qcom,msm-cdc-pinctrl";
		pinctrl-names = "aud_active", "aud_sleep";
		pinctrl-0 = <&cdc_dmic23_clk_active &cdc_dmic23_data_active>;
		pinctrl-1 = <&cdc_dmic23_clk_sleep &cdc_dmic23_data_sleep>;
	};
};

&camss_cpp_gdsc {
	qcom,support-hw-trigger;
};

&qupv3_se1_i2c {
	pinctrl-0 = <&sm_gpio_4 &sm_gpio_5>;
	pinctrl-1 = <&sm_gpio_4_suspend &sm_gpio_5_suspend>;
	status = "okay";

	nq@28 {
		status = "disabled";
	};

	nfc@28 {
		compatible = "nxp,pn553";
		reg = <0x28>;
		interrupt-parent = <&tlmm>;
		interrupts = <85 0x1>;
		nxp,irq_gpio = <&tlmm 85 0x00>;
		nxp,dwld_en = <&tlmm 84 0x00>;
		nxp,ven = <&tlmm 83 0x01>;
		nxp,ese-pwr = <&tlmm 24 0x00>;
	};
};

/* Disable trinket-audio-overlay unused nodes */
&cdc_dmic01_gpios {
	status = "disabled";
};

&cdc_dmic23_gpios {
	status = "disabled";
};

&sm6150_snd {
	asoc-codec  = <&stub_codec>, <&bolero>, <&ext_disp_audio_codec>,
				<&cs35l41>;
	qcom,pri-mi2s-gpios = <&cdc_pri_mi2s_gpios>;
	qcom,cdc-dmic01-gpios = <&pri_dmic_gpios>;
	qcom,cdc-dmic23-gpios = <&sec_dmic_gpios>;
	asoc-codec-names = "msm-stub-codec.1", "bolero_codec",
				"msm-ext-disp-audio-codec-rx", "cs35l41.2-0040";

	qcom,wsa-max-devs = <0>;
};

&dai_mi2s0 {
	qcom,msm-mi2s-rx-lines = <1>;
	qcom,msm-mi2s-tx-lines = <2>;
};

&qupv3_se3_i2c {
	pinctrl-0 = <&sm_gpio_14 &sm_gpio_15>;
	pinctrl-1 = <&sm_gpio_14_suspend &sm_gpio_15_suspend>;
	qcom,clk-freq-out = <1000000>;
	status = "okay";

	cs35l41: cs35l41@40 {
		compatible = "cirrus,cs35l41";
		reg = <0x40>;
		interrupt-parent = <&tlmm>;
		interrupts = <27 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 26 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&sm_gpio_27>, <&sm_gpio_26>;
		cirrus,boost-peak-milliamp = <4000>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,hibernate-enable = <1>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio-config2 {
			cirrus,gpio-src-select = <0x2>;
			cirrus,gpio-output-enable;
		};
	};
};
/* Sound card registration may fail */
&bolero {
	qcom,num-macros = <3>;
};

&wsa_macro {
	status = "disabled";
};

&pm8008_8 {
	status = "disabled";
};

&pm8008_9 {
	status = "disabled";
