&spmi_bus {
	qcom,pmk8350@0 {
		pon_hlos@1300 {
			qcom,pon_2 {
				linux,code = <KEY_VOLUMEUP>;
			};
		};
	};
};

&soc {
	gpio_keys {
		pinctrl-0 = <&pmk8350_gpio_3
			     &pm8350_gpio_9
			     &pm8350b_gpio_5
			     &pm8350b_gpio_8>;

		vol_up {
			status = "disabled";
		};

		vol_dn {
			label = "volume_down";
			gpios = <&pmk8350_gpios 3 GPIO_ACTIVE_LOW>;
