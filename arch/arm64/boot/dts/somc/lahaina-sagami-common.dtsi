&soc {
	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = <&sm_gpio_0 &sm_gpio_1 &sm_gpio_2 &sm_gpio_3 &sm_gpio_6 &sm_gpio_7
			     &sm_gpio_8 &sm_gpio_9 &sm_gpio_10 &sm_gpio_11 &sm_gpio_12
			     &sm_gpio_13 &sm_gpio_14 &sm_gpio_15 &sm_gpio_16 &sm_gpio_17
			     &sm_gpio_20 &sm_gpio_21 /*&sm_gpio_22 &sm_gpio_23 &sm_gpio_24*/
			     &sm_gpio_25 &sm_gpio_26 &sm_gpio_27 &sm_gpio_28 &sm_gpio_29
			     &sm_gpio_30 &sm_gpio_31 &sm_gpio_32 &sm_gpio_33 &sm_gpio_34
			     &sm_gpio_35 &sm_gpio_36 &sm_gpio_37 &sm_gpio_38 &sm_gpio_39
			     &sm_gpio_40 &sm_gpio_41 &sm_gpio_42 &sm_gpio_43 &sm_gpio_48
			     &sm_gpio_49 &sm_gpio_50 &sm_gpio_51 &sm_gpio_52 &sm_gpio_53
			     &sm_gpio_54 &sm_gpio_55 &sm_gpio_56 &sm_gpio_57 &sm_gpio_58
			     &sm_gpio_59 &sm_gpio_60 &sm_gpio_61 &sm_gpio_62 &sm_gpio_63
			     &sm_gpio_66 &sm_gpio_67 &sm_gpio_72 &sm_gpio_73 &sm_gpio_74
			     &sm_gpio_75 &sm_gpio_76 &sm_gpio_77 &sm_gpio_78 &sm_gpio_79
			     &sm_gpio_80 /*&sm_gpio_82*/ &sm_gpio_83 &sm_gpio_84 &sm_gpio_85
			     &sm_gpio_86 &sm_gpio_87 &sm_gpio_89 &sm_gpio_90 &sm_gpio_91
			     &sm_gpio_92 &sm_gpio_97 &sm_gpio_98 &sm_gpio_99_suspend &sm_gpio_100
			     &sm_gpio_101 &sm_gpio_102 &sm_gpio_103 &sm_gpio_104 &sm_gpio_105
			     &sm_gpio_106 &sm_gpio_107 &sm_gpio_108 &sm_gpio_109 &sm_gpio_110
			     &sm_gpio_111 &sm_gpio_112 &sm_gpio_113 &sm_gpio_114 &sm_gpio_115
			     &sm_gpio_116 &sm_gpio_117 &sm_gpio_119 &sm_gpio_120 &sm_gpio_121
			     &sm_gpio_122 &sm_gpio_123 &sm_gpio_124 &sm_gpio_125 &sm_gpio_126
			     &sm_gpio_127 &sm_gpio_128 &sm_gpio_131 &sm_gpio_132 /*&sm_gpio_154*/
			     &sm_gpio_155 &sm_gpio_156 &sm_gpio_157 &sm_gpio_158 &sm_gpio_162
			     &sm_gpio_163 &sm_gpio_164 &sm_gpio_165 &sm_gpio_166 &sm_gpio_167
			     &sm_gpio_183 &sm_gpio_184 &sm_gpio_185 &sm_gpio_186 &sm_gpio_195
			     &sm_gpio_196 &sm_gpio_197 &sm_gpio_198>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pmk8350_gpio_1 &pmk8350_gpio_2 /*&pmk8350_gpio_3*/ &pm8350_gpio_1 &pm8350_gpio_2
			     &pm8350_gpio_4 &pm8350_gpio_5 &pm8350_gpio_6 &pm8350_gpio_8 /*&pm8350_gpio_9*/
			     &pm8350b_gpio_1 &pm8350b_gpio_2 &pm8350b_gpio_3 &pm8350b_gpio_4 /*&pm8350b_gpio_5*/
			     &pm8350b_gpio_6 &pm8350b_gpio_7 /*&pm8350b_gpio_8*/ &pm8350c_gpio_1 &pm8350c_gpio_2
			     &pm8350c_gpio_3 &pm8350c_gpio_5 &pm8350c_gpio_6 &pm8350c_gpio_7 &pm8350c_gpio_8
			     &pm8350c_gpio_9>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	egistec_vcc_pm8350_gpio_6: egistec-vcc-pm8350-gpio-6 {
		compatible = "regulator-fixed";
		regulator-name = "egistec-vcc-pm8350-gpio-6";
		regulator-min-microvolt = <3800000>;
		regulator-max-microvolt = <3800000>;
		startup-delay-us = <2000>;
		enable-active-high;
		gpio = <&pm8350_gpios 6 0>;

		/*
		 * Use a "parent" supply just to be able to turn on this VCC gpio and
		 * the pm8350c BoB (switched on by GPIO 6) regulator */
		vin-supply = <&pm8350c_bob>;
	};

	et6xx: et6xx {
		compatible = "egistec,et580";

		et51x,gpio_irq = <&tlmm 38 0x0>;
		et51x,no-low-voltage-probe;
		et51x,vana-voltage = <3800000>;
		vdd_ana-supply = <&egistec_vcc_pm8350_gpio_6>;

		pinctrl-names = "et51x_reset_reset",
				"et51x_reset_active",
				"et51x_irq_active";
		pinctrl-0 = <&EGIS_RST_sleep>;
		pinctrl-1 = <&EGIS_RST_active>;
		pinctrl-2 = <&sm_gpio_38_input_no_pull>;
		status = "ok";
	};

	/* Disable extcon_usb1 */
	extcon_usb1 {
		status = "disabled";
	};

	last_logs@ffbc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};

	reboot_reason {
		qcom,use_custom_reasons;
	};

	restart-reason {
		none {
			cmd = "none";
			reg-val = <0x00 0x00000000>;
		};

		unknown {
			cmd = "unknown";
			reg-val = <0x01 0x77665501>;
		};

		recovery {
			cmd = "recovery";
			reg-val = <0x02 0x77665500>;
			reboot-cmd;
		};

		bootloader {
			cmd = "bootloader";
			reg-val = <0x03 0x77665500>;
			reboot-cmd;
		};

		rtc {
			cmd = "rtc";
			reg-val = <0x04 0x77665500>;
			reboot-cmd;
		};

		dm-verity-device-corrupted {
			cmd = "dm-verity device corrupted";
			reg-val = <0x05 0x77665508>;
			reboot-cmd;
		};

		dm-verity-enforcing {
			cmd = "dm-verity enforcing";
			reg-val = <0x06 0x77665509>;
			reboot-cmd;
		};

		keys-clear {
			cmd = "keys clear";
			reg-val = <0x07 0x7766550A>;
			reboot-cmd;
		};

		panic {
			cmd = "panic";
			reg-val = <0x40 0xC0DEDEAD>;
		};

		oem-N {
			cmd = "oem-4E";
			reg-val = <0x61 0x6F656D4E>;
			reboot-cmd;
		};

		oem-S {
			cmd = "oem-53";
			reg-val = <0x60 0x6F656D53>;
			reboot-cmd;
		};

		oem-P {
			cmd = "oem-50";
			reg-val = <0x51 0x6F656D50>;
			reboot-cmd;
		};
	};

	sony_ext_uim_ctrl {
		compatible = "sony,ext-uim-ctrl";
		uim2_detect_en_gpio = <&tlmm 132 GPIO_ACTIVE_HIGH>;
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 80 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	rgbcir_vreg: rgbcir_vreg {
		compatible = "regulator-fixed";
		regulator-name = "rgbcir_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pm8350c_gpios 6 0>;
	};
};

&tlmm {
	EGIS_RST_active: EGIS_RST_active {
		mux {
			pins = "gpio39";
			function = "gpio";
		};

		config {
			pins = "gpio39";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	EGIS_RST_sleep: EGIS_RST_sleep {
		mux {
			pins = "gpio39";
			function = "gpio";
		};

		config {
			pins = "gpio39";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
};

&pm8350_gpios {
	EGIS_VCC_active: EGIS_VCC_active {
		pins = "gpio6";
		function = "normal";
		output-high;
		bias-disable;
		power-source = <0>;
	};

	EGIS_VCC_sleep: EGIS_VCC_sleep {
		pins = "gpio6";
		function = "normal";
		output-low;
		bias-disable;
		power-source = <0>;
	};
};

&wil6210 {
	status = "disabled";
};

&qupv3_se2_i2c {
	pinctrl-0 = <&sm_gpio_12 &sm_gpio_13>;
	pinctrl-1 = <&sm_gpio_12_suspend &sm_gpio_13_suspend>;
	status = "okay";
	qcom,clk-freq-out = <400000>;
		rtc6226@64 {
			compatible = "richwave,rtc6226";
			reg = <0x64>;
			fmint-gpio = <&tlmm 50 0>;
			vdd-supply = <&L7E>;
			rtc6226,vdd-supply-voltage = <2800000 2800000>;
			rtc6226,vdd-load = <30000>;
			vio-supply = <&S10B>;
			rtc6226,vio-supply-voltage = <1800000 1800000 >;
		};
};

&qupv3_se4_i2c {
	pinctrl-0 = <&sm_gpio_20 &sm_gpio_21>;
	pinctrl-1 = <&sm_gpio_20_suspend &sm_gpio_21_suspend>;
	qcom,clk-freq-out = <400000>;
	status = "okay";
};

&qupv3_se11_i2c {
	pinctrl-0 = <&sm_gpio_48 &sm_gpio_49>;
	pinctrl-1 = <&sm_gpio_48_suspend &sm_gpio_49_suspend>;
	qcom,clk-freq-out = <1000000>;
	status = "okay";

	cs35l41_l: L-cs35l41@40 {
		compatible = "cirrus,cs35l41";
		reg = <0x40>;
		interrupt-parent = <&tlmm>;
		interrupts = <36 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&sm_gpio_36>, <&sm_gpio_37>;
		cirrus,boost-peak-milliamp = <4000>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio-config2 {
			cirrus,gpio-src-select = <0x2>;
			cirrus,gpio-output-enable;
		};
	};

	cs35l41_r: R-cs35l41@41 {
		compatible = "cirrus,cs35l41";
		reg = <0x41>;
		interrupt-parent = <&tlmm>;
		interrupts = <36 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
		cirrus,right-channel-amp;
		cirrus,boost-peak-milliamp = <4000>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio-config2 {
			cirrus,gpio-src-select = <0x2>;
			cirrus,gpio-output-enable;
		};
	};
};

