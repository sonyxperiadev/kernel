&lpi_tlmm {
	somc_cdc_dmic01_clk_active: somc_dmic01_clk_active {
		mux {
			pins = "gpio6";
			function = "func1";
		};

		config {
			pins = "gpio6";
			drive-strength = <2>;
			output-high;
		};
	};

	somc_cdc_dmic01_clk_sleep: somc_dmic01_clk_sleep {
		mux {
			pins = "gpio6";
			function = "func1";
		};

		config {
			pins = "gpio6";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	somc_cdc_dmic01_data_active: somc_dmic01_data_active {
		mux {
			pins = "gpio7";
			function = "func1";
		};

		config {
			pins = "gpio7";
			drive-strength = <2>;
			input-enable;
		};
	};

	somc_cdc_dmic01_data_sleep: somc_dmic01_data_sleep {
		mux {
			pins = "gpio7";
			function = "func1";
		};

		config {
			pins = "gpio7";
			drive-strength = <2>;
			pull-down;
			input-enable;
		};
	};

	somc_cdc_dmic23_clk_active: somc_dmic23_clk_active {
