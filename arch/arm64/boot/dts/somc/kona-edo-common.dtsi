&qupv3_se1_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	pinctrl-0 = <&sm_gpio_4 &sm_gpio_5>;
	pinctrl-1 = <&sm_gpio_4_suspend &sm_gpio_5_suspend>;
	qcom,clk-freq-out = <400000>;
	status = "okay";
};

&qupv3_se2_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	pinctrl-0 = <&sm_gpio_115 &sm_gpio_116>;
	pinctrl-1 = <&sm_gpio_115_suspend &sm_gpio_116_suspend>;
	qcom,clk-freq-out = <1000000>;
	status = "okay";

	cs35l41_l: cs35l41@40 {
		compatible = "cirrus,cs35l41";
		reg = <0x40>;
		interrupt-parent = <&tlmm>;
		interrupts = <11 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 10 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&sm_gpio_11>, <&sm_gpio_10>;
		cirrus,boost-peak-milliamp = <4000>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,tuning-has-prefix = <1>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio-config2 {
			cirrus,gpio-src-select = <0x2>;
			cirrus,gpio-output-enable;
		};
	};

	cs35l41_r: cs35l41@41 {
		compatible = "cirrus,cs35l41";
		reg = <0x41>;
		interrupt-parent = <&tlmm>;
		interrupts = <11 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 10 GPIO_ACTIVE_HIGH>;
		cirrus,right-channel-amp;
		cirrus,boost-peak-milliamp = <4000>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,tuning-has-prefix = <1>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio-config2 {
			cirrus,gpio-src-select = <0x2>;
			cirrus,gpio-output-enable;
		};
	};
};

