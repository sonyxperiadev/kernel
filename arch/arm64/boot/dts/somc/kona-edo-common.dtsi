&spmi_bus {
	qcom,pm8150@0 {
		qcom,power-on@800 {
			qcom,pon_2 {
				linux,code = <KEY_VOLUMEUP>;
			};
		};
	};
};

&soc {
	gpio_keys {
		pinctrl-0 = <&pm8150_gpio_1
			     &pm8150b_gpio_1
			     &pm8150b_gpio_2>;
		vol_dn {
			label = "volume_down";
			gpios = <&pm8150_gpios 1 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		cam_snapshot {
			label = "cam_snapshot";
			gpios = <&pm8150b_gpios 1 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <766>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		cam_focus {
			label = "cam_focus";
			gpios = <&pm8150b_gpios 2 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_CAMERA_FOCUS>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		vol_up {
			status = "disabled";
		};
	};

	somc_water_detection: somc_water_detection {
		compatible = "somc,water-detection";
		dvdt_enable_gpio = <&tlmm 82 0>;
		dvdt_wrt_det_and = <&tlmm 9 0>;
		dvdt_wrt_det_or = <&tlmm 83 0>;
	};

	sony_ext_uim_ctrl: sony_ext_uim_ctrl {
		compatible = "sony,ext-uim-ctrl";
		uim2_detect_en_gpio = <&tlmm 47 GPIO_ACTIVE_HIGH>;
	};
};

/*
 * Disable extcon_usb1
 * to keep GPIO(PMIC/PM8150) in HW Configuration Specification
 */
&vendor {
	extcon_usb1 {
		status = "disabled";
	};
};

&usb1 {
	extcon = <0>;
	status = "disabled";
};

