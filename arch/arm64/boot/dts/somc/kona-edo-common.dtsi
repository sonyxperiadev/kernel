&soc {

	pinctrl@f000000 {
		disabled-pins = <40 41 42 43 52 53 54 55>;
	};

	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = <&sm_gpio_4 &sm_gpio_5 &sm_gpio_6 &sm_gpio_7 &sm_gpio_8
			     &sm_gpio_9 &sm_gpio_10 &sm_gpio_11 &sm_gpio_12 &sm_gpio_13
			     &sm_gpio_14 &sm_gpio_15 &sm_gpio_22 &sm_gpio_23 &sm_gpio_24
			     &sm_gpio_25 &sm_gpio_26 &sm_gpio_27 &sm_gpio_28 &sm_gpio_29
			     &sm_gpio_30 &sm_gpio_31 &sm_gpio_33 &sm_gpio_36 &sm_gpio_37
			     /*&sm_gpio_38*/ /*&sm_gpio_39*/ &sm_gpio_44 &sm_gpio_45
			     &sm_gpio_46 &sm_gpio_47 &sm_gpio_48 &sm_gpio_49 &sm_gpio_50
			     &sm_gpio_51 &sm_gpio_60 &sm_gpio_61 &sm_gpio_62 &sm_gpio_63
			     &sm_gpio_64 &sm_gpio_66 &sm_gpio_67 &sm_gpio_68 &sm_gpio_69
			     &sm_gpio_70 &sm_gpio_71 &sm_gpio_72 &sm_gpio_73 &sm_gpio_74
			     /*&sm_gpio_75*/ &sm_gpio_76 &sm_gpio_77 &sm_gpio_78 &sm_gpio_82
			     &sm_gpio_83 &sm_gpio_84 &sm_gpio_90 &sm_gpio_91 &sm_gpio_92
			     &sm_gpio_93 &sm_gpio_94 &sm_gpio_95 &sm_gpio_96 &sm_gpio_97
			     &sm_gpio_98 &sm_gpio_99 &sm_gpio_100 &sm_gpio_101 &sm_gpio_102
			     &sm_gpio_103 &sm_gpio_104 &sm_gpio_105 &sm_gpio_106 &sm_gpio_107
			     &sm_gpio_108 &sm_gpio_109 &sm_gpio_110 &sm_gpio_111 &sm_gpio_112
			     &sm_gpio_113 &sm_gpio_114 &sm_gpio_115 &sm_gpio_116 &sm_gpio_117
			     &sm_gpio_118 &sm_gpio_119 &sm_gpio_120 &sm_gpio_121 &sm_gpio_122
			     &sm_gpio_125 &sm_gpio_126 &sm_gpio_128 &sm_gpio_129 &sm_gpio_130
			     &sm_gpio_131 &sm_gpio_132 &sm_gpio_133 &sm_gpio_134 &sm_gpio_135
			     &sm_gpio_136 &sm_gpio_137 &sm_gpio_138_suspend &sm_gpio_139 &sm_gpio_140
			     &sm_gpio_141 &sm_gpio_144 &sm_gpio_145 &sm_gpio_162 &sm_gpio_163
			     &sm_gpio_168 &sm_gpio_169 &sm_gpio_172 &sm_gpio_173 &sm_gpio_174
			     &sm_gpio_175>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pm8150_gpio_1 &pm8150_gpio_3 &pm8150_gpio_6 &pm8150_gpio_7
			     &pm8150_gpio_9 &pm8150_gpio_10 &pm8150b_gpio_1 &pm8150b_gpio_2
			     &pm8150b_gpio_3 &pm8150b_gpio_4 &pm8150b_gpio_5 &pm8150b_gpio_6
			     &pm8150b_gpio_7 &pm8150b_gpio_8 &pm8150b_gpio_9 &pm8150b_gpio_10
			     &pm8150b_gpio_11 &pm8150b_gpio_12 &pm8150l_gpio_1 &pm8150l_gpio_3
			     &pm8150l_gpio_4 &pm8150l_gpio_5 &pm8150l_gpio_6 &pm8150l_gpio_8
			     &pm8150l_gpio_10 &pm8150l_gpio_11 &pm8009_gpio_1 &pm8009_gpio_2
			     &pm8009_gpio_3 &pm8009_gpio_4>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	qcom,qbt_handler {
		status = "disabled";
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 121 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	last_logs@ffbc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};

	restart-reason {
		none {
			cmd = "none";
			reg-val = <0x00 0x00000000>;
		};

		unknown {
			cmd = "unknown";
			reg-val = <0x01 0x77665501>;
		};

		recovery {
			cmd = "recovery";
			reg-val = <0x02 0x77665500>;
			reboot-cmd;
		};

		bootloader {
			cmd = "bootloader";
			reg-val = <0x03 0x77665500>;
			reboot-cmd;
		};

		rtc {
			cmd = "rtc";
			reg-val = <0x04 0x77665500>;
			reboot-cmd;
		};

		dm-verity-device-corrupted {
			cmd = "dm-verity device corrupted";
			reg-val = <0x05 0x77665508>;
			reboot-cmd;
		};

		dm-verity-enforcing {
			cmd = "dm-verity enforcing";
			reg-val = <0x06 0x77665509>;
			reboot-cmd;
		};

		keys-clear {
			cmd = "keys clear";
			reg-val = <0x07 0x7766550A>;
			reboot-cmd;
		};

		panic {
			cmd = "panic";
			reg-val = <0x40 0xC0DEDEAD>;
		};

		oem-N {
			cmd = "oem-4E";
			reg-val = <0x61 0x6F656D4E>;
			reboot-cmd;
		};

		oem-S {
			cmd = "oem-53";
			reg-val = <0x60 0x6F656D53>;
			reboot-cmd;
		};

		oem-P {
			cmd = "oem-50";
			reg-val = <0x51 0x6F656D50>;
			reboot-cmd;
		};

		fota_crash {
			cmd = "fota_crash";
			reg-val = <0x42 0xABADF00D>;
			reboot-cmd;
		};

		systemdump_success {
			cmd = "systemdump_success";
			reg-val = <0x49 0x000052D1>;
			reboot-cmd;
		};

		systemdump_decryptextsd {
			cmd = "systemdump_decryptextsd";
			reg-val = <0x4A 0x000052D2>;
			reboot-cmd;
		};

		systemdump_decryptfail {
			cmd = "systemdump_decryptfail";
			reg-val = <0x4B 0x000052D3>;
			reboot-cmd;
		};

		systemdump_hwwd {
			cmd = "systemdump_hwwd";
			reg-val = <0x4C 0x000052D4>;
			reboot-cmd;
		};

		systemdump_nospace {
			cmd = "systemdump_nospace";
			reg-val = <0x4D 0x000052D5>;
			reboot-cmd;
		};

		systemdump_panic {
			cmd = "systemdump_panic";
			reg-val = <0x4E 0x000052DC>;
			reboot-cmd;
		};

		systemdump_err {
			cmd = "systemdump_err";
			reg-val = <0x4F 0x000052DE>;
			reboot-cmd;
		};
	};

	fpc_vana_vreg: fpc_vana_vreg {
		compatible = "regulator-fixed";
		regulator-name = "fpc_vana_fixed_reg";
		regulator-min-microvolt = <3350000>;
		regulator-max-microvolt = <3350000>;
		startup-delay-us = <2000>;
		enable-active-high;
		gpio = <&pm8150l_gpios 8 0>;
		vin-supply = <&pm8150a_bob>;
	};

	et6xx: et6xx {
		compatible = "egistec,et580";

		et51x,gpio_irq = <&tlmm 23 0x0>;
		et51x,no-low-voltage-probe;
		et51x,vana-voltage = <3350000>;
		vdd_ana-supply = <&fpc_vana_vreg>;

		pinctrl-names = "et51x_reset_reset",
				"et51x_reset_active",
				"et51x_irq_active";
		pinctrl-0 = <&sm_gpio_74>;
		pinctrl-1 = <&sm_gpio_74_output_high>;
		pinctrl-2 = <&sm_gpio_23>;
		status = "ok";
	};

	rgbcir_vreg: rgbcir_vreg {
		compatible = "regulator-fixed";
		regulator-name = "rgbcir_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&tlmm 22 0>;
	};

	cam0_vdig_vreg: cam0_vdig_vreg {
		compatible = "regulator-fixed";
		regulator-name = "camera0_vdig_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pm8009_gpios 3 0>;
	};

	cam4_vdig_vreg: cam4_vdig_vreg {
		compatible = "regulator-fixed";
		regulator-name = "camera4_vdig_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pm8009_gpios 2 0>;
		vin-supply = <&slg51000_0_ldo5>;
	};

};

&cam_cci0 {
	/* The value of clock that depens on the following parameter is different from Qualcomm.
	Change the value for Somc. */
	qcom,i2c_fast_mode {
		hw-thigh = <43>;
		hw-tlow = <64>;
		hw-tsu-sto = <41>;
		hw-tsu-sta = <41>;
		hw-thd-dat = <25>;
		hw-thd-sta = <35>;
		hw-tbuf = <64>;
		hw-scl-stretch-en = <0>;
		hw-trdhld = <6>;
		hw-tsp = <3>;
		cci-clk-src = <37500000>;
		status = "ok";
	};
	qcom,i2c_fast_plus_mode {
		hw-thigh = <16>;
		hw-tlow = <22>;
		hw-tsu-sto = <17>;
		hw-tsu-sta = <18>;
		hw-thd-dat = <16>;
		hw-thd-sta = <15>;
		hw-tbuf = <19>;
		hw-scl-stretch-en = <1>;
		hw-trdhld = <3>;
		hw-tsp = <3>;
		cci-clk-src = <37500000>;
		status = "ok";
	};

	qcom,cam-sensor0 {
		status = "disabled";
	};
	qcom,cam-sensor1 {
		status = "disabled";
	};
	qcom,cam-sensor4 {
		status = "disabled";
	};
	qcom,cam-sensor5 {
		status = "disabled";
	};
};

