&glink_adsp {
	qcom,adsp_qrtr {
		qcom,non-wake-svc = <0x190>;
	};
};

&usb0 {
	dwc3@4e00000 {
		usb-phy = <&usb2_phy0>, <&usb_nop_phy>;
		maximum-speed = "high-speed";
	};

	ssphy@1630000 {
		status = "disabled";
	};
};

&pm6125_pon {
	qcom,pon_2 {
		linux,code = <KEY_VOLUMEUP>;
	};
};

&pm6125_pwm {
	status = "okay";
};

&pm6125_vadc {
	pinctrl-0 = <
			&pa_therm2_default
			&msm_therm_default
			&rfc_cam_therm_default
			&flash_cam_therm_default>;

	/delete-node/ rear_cam_flash_therm;

	flash_cam_therm {
		reg = <ADC5_GPIO3_100K_PU>;
		label = "flash_cam_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm6125_gpios {
	/delete-node/ rear_cam_flash_therm;

	flash_cam_therm {
		flash_cam_therm_default: flash_cam_therm_default {
			pins = "gpio6";
			bias-high-impedance;
		};
	};
};

&pm6125_adc_tm {
	/delete-node/ rear_cam_flash_therm;

	flash_cam_therm {
		reg = <ADC5_GPIO3_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&pm7250b_vadc {
	pinctrl-names = "default";
	pinctrl-0 = <&get_stageid_adc_mb_default>;

	get_stageid_adc_mb {
		reg = <ADC5_AMUX_THM4_100K_PU>;
		label = "get_stageid_adc_mb";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm7250b_gpios {
	get_stageid_adc_mb {
		get_stageid_adc_mb_default: get_stageid_adc_mb_default {
			pins = "gpio1";
			bias-high-impedance;
		};
	};
};

&thermal_zones {
	/delete-node/ camera-therm-usr;

	soc {
		trips {
			low-soc {
				temperature = <5>;
				hysteresis = <0>;
				type = "passive";
			};
		};
	};

	flash-cam-therm-usr {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC5_GPIO3_100K_PU>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};
};
