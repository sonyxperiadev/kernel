	};

	/* GPIO_6: NC */
	pm8150b_gpio_6: pm8150b_gpio_6 {
		pins = "gpio6";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_6 */
	};

	/* GPIO_7: TS_VDDH_EN */
	pm8150b_gpio_7: pm8150b_gpio_7 {
		pins = "gpio7";
		function = "normal";
		output-low;
		drive-push-pull;
		bias-disable;
		qcom,drive-strength = <1>; /* Low */
		power-source = <1>;
	};

	/* GPIO_8: LCD_ID */
	pm8150b_gpio_8: pm8150b_gpio_8 {
		pins = "gpio8";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_8 Analog PIN*/
	};

	/* GPIO_10: NC */
	pm8150b_gpio_10: pm8150b_gpio_10 {
		pins = "gpio10";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_10 */
	};

	/* GPIO_11: NC */
	pm8150b_gpio_11: pm8150b_gpio_11 {
		pins = "gpio11";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_11 */
	};

	/* GPIO_12: RF_ID */
	pm8150b_gpio_12: pm8150b_gpio_12 {
		pins = "gpio12";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_12 Analog PIN */
	};
};

&pm8150l_gpios {

	/delete-property/ qcom,gpios-disallowed;
	interrupts = <0x4 0xc0 0 IRQ_TYPE_NONE>,
			<0x4 0xc1 0 IRQ_TYPE_NONE>,
			<0x4 0xc2 0 IRQ_TYPE_NONE>,
			<0x4 0xc3 0 IRQ_TYPE_NONE>,
			<0x4 0xc4 0 IRQ_TYPE_NONE>,
