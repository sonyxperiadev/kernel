#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/phy/qcom,sm8150-qmp-usb3.h>

&reserved_memory {
	#address-cells = <2>;
	#size-cells = <2>;

	ramoops: ramoops@ffc00000 {
		compatible = "removed-dma-pool", "ramoops";
		no-map;
		reg = <0 0xffc00000 0 0x00100000>;
		record-size = <0x1000>;
		console-size = <0x40000>;
		ftrace-size = <0x0>;
		msg-size = <0x20000>;
		cc-size = <0x16>;
	};
};

&soc {
	qcom,pcie@1c00000 {
		status = "disabled";
	};

	qcom,pcie@1c08000 {
		status = "disabled";
	};

	pinctrl@03000000 {
		disabled-pins = <126 127 128 129>;

		cci0_active {
			config {
				/delete-property/ bias-pull-up; /* PULL UP*/
				bias-disable; /* No PULL */
			};
		};

		cci1_active {
			config {
				/delete-property/ bias-pull-up; /* PULL UP*/
				bias-disable; /* No PULL */
			};
		};

		cci2_active {
			config {
				/delete-property/ bias-pull-up; /* PULL UP*/
				bias-disable; /* No PULL */
			};
		};

		cci3_active {
			config {
				/delete-property/ bias-pull-up; /* PULL UP*/
				bias-disable; /* No PULL */
			};
		};

		/* HW_ID_0/1 to use correctly */
		hw_id_pins: hw_id_pins {
			hw_id_active: hw_id_active {
				mux {
					pins = "gpio59", "gpio139";
					function = "gpio";
				};

				config {
					pins = "gpio59", "gpio139";
					drive-strength = <2>;
					bias-pull-down;
					input-enable;
				};
			};

			hw_id_sleep: hw_id_sleep {
				mux {
					pins = "gpio59", "gpio139";
					function = "gpio";
				};

				config {
					pins = "gpio59", "gpio139";
					drive-strength = <2>;
					bias-pull-up;
					input-enable;
				};
			};
		};
	};

	rgbcir_vreg: rgbcir_vreg {
		compatible = "regulator-fixed";
		regulator-name = "rgbcir_vreg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&tlmm 29 0>;
	};

	gpio_keys {
		pinctrl-0 = <&key_vol_dn_default>;

		vol_dn {
			label = "volume_down";
			gpios = <&pm8150_gpios 1 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
	};

	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = </*&sm_gpio_0*/ &sm_gpio_1 &sm_gpio_2 &sm_gpio_3
			     &sm_gpio_4 &sm_gpio_5 /*&sm_gpio_6*/ &sm_gpio_7 &sm_gpio_8
			     &sm_gpio_9 &sm_gpio_10 &sm_gpio_11 &sm_gpio_12 &sm_gpio_13
			     &sm_gpio_14 &sm_gpio_15 &sm_gpio_16 &sm_gpio_17 &sm_gpio_18
			     &sm_gpio_19 &sm_gpio_20 &sm_gpio_21 &sm_gpio_22 &sm_gpio_23
			     &sm_gpio_24 &sm_gpio_25 &sm_gpio_26 &sm_gpio_27 &sm_gpio_28
			     &sm_gpio_29 &sm_gpio_30 &sm_gpio_31 &sm_gpio_32 &sm_gpio_33
			     &sm_gpio_34 &sm_gpio_35 &sm_gpio_36 &sm_gpio_37_suspend &sm_gpio_39
			     &sm_gpio_40 &sm_gpio_41 &sm_gpio_42 &sm_gpio_47 &sm_gpio_48
			     &sm_gpio_49 &sm_gpio_51 &sm_gpio_52 &sm_gpio_53 /*&sm_gpio_54*/
			     &sm_gpio_55 &sm_gpio_56 &sm_gpio_57 &sm_gpio_58 &sm_gpio_59
			     &sm_gpio_60 &sm_gpio_65 &sm_gpio_66 &sm_gpio_67 &sm_gpio_79
			     &sm_gpio_80 &sm_gpio_81 &sm_gpio_82 &sm_gpio_83 &sm_gpio_84
			     &sm_gpio_85 &sm_gpio_86 &sm_gpio_87 &sm_gpio_88 &sm_gpio_89
			     &sm_gpio_90 &sm_gpio_91 &sm_gpio_92 &sm_gpio_93 &sm_gpio_94
			     &sm_gpio_96 &sm_gpio_97 &sm_gpio_98 &sm_gpio_99
			     &sm_gpio_100 &sm_gpio_101 &sm_gpio_102 &sm_gpio_103 &sm_gpio_104
			     &sm_gpio_105 &sm_gpio_106 &sm_gpio_107 &sm_gpio_108 &sm_gpio_109
			     &sm_gpio_110 &sm_gpio_111 &sm_gpio_113 &sm_gpio_114
			     &sm_gpio_115 &sm_gpio_116 &sm_gpio_117 &sm_gpio_118 &sm_gpio_119
			     &sm_gpio_120 &sm_gpio_121 /*&sm_gpio_122*/ &sm_gpio_125 &sm_gpio_130
			     &sm_gpio_131 &sm_gpio_133 &sm_gpio_134 &sm_gpio_135 &sm_gpio_136
			     &sm_gpio_137 &sm_gpio_138 &sm_gpio_139 &sm_gpio_140 &sm_gpio_141
			     &sm_gpio_142 &sm_gpio_144 &sm_gpio_145 &sm_gpio_146 &sm_gpio_147
			     &sm_gpio_148 &sm_gpio_152 &sm_gpio_163 &sm_gpio_164 &sm_gpio_165
			     &sm_gpio_166>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pm8150_gpio_1 &pm8150_gpio_3 &pm8150_gpio_4 &pm8150_gpio_6
			    &pm8150_gpio_7 &pm8150_gpio_9 &pm8150_gpio_10 &pm8150b_gpio_1
			    &pm8150b_gpio_2 &pm8150b_gpio_3 &pm8150b_gpio_4 &pm8150b_gpio_5 &pm8150b_gpio_6
			    &pm8150b_gpio_7 &pm8150b_gpio_8 &pm8150b_gpio_10 &pm8150b_gpio_11 &pm8150b_gpio_12
			    &pm8150l_gpio_1 &pm8150l_gpio_2 &pm8150l_gpio_3 &pm8150l_gpio_4 &pm8150l_gpio_6
			    &pm8150l_gpio_7 &pm8150l_gpio_8 &pm8150l_gpio_9 &pm8150l_gpio_10 &pm8150l_gpio_11
			    &pm8150l_gpio_12>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	pm8150l_gpio_8_reg: pm8150l_gpio_8_reg {
		compatible = "regulator-fixed";
		regulator-name = "pm8150l_gpio_8_reg";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pm8150l_gpios 8 0>;
	};

	et6xx {
		compatible = "etspi,et510";
		interrupt-parent = <&tlmm>;
		interrupts = <118 0x0>;
		et51x,gpio_irq = <&tlmm 118 0>;
		vdd_ana-supply = <&pm8150l_gpio_8_reg>;
		et51x,no-low-voltage-probe;

		pinctrl-names = "et51x_reset_reset",
			"et51x_reset_active",
			"et51x_irq_active";
		pinctrl-0 = <&sm_gpio_80>;
		pinctrl-1 = <&sm_gpio_80_output_high>;
		pinctrl-2 = <&sm_gpio_118>;
	};

	sony_ext_uim_ctrl {
		compatible = "sony,ext-uim-ctrl";
		uim2-gpios = <&tlmm 49 GPIO_ACTIVE_HIGH>;
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&tlmm 121 0x1>;
			lid-pin = <1>;
			open-debounce-interval = <120>;
			close-debounce-interval = <300>;
		};
	};

	wcd_usbc_analog_en1_gpio: msm_cdc_pinctrl@100 {
		compatible = "qcom,msm-cdc-pinctrl";
		pinctrl-names = "aud_active", "aud_sleep";
		pinctrl-0 = <&wcd_usbc_analog_en1_active>;
		pinctrl-1 = <&wcd_usbc_analog_en1_idle>;
	};

	/* SPI: QUP3 */
	spi@88c000 {
		pinctrl-0 = <&sm_gpio_144 &sm_gpio_145 &sm_gpio_146 &sm_gpio_147>;
		status = "okay";
	};

	/* I2C: QUP4 */
	i2c@890000 {
		pinctrl-0 = <&sm_gpio_51 &sm_gpio_52>;
		pinctrl-1 = <&sm_gpio_51_suspend &sm_gpio_52_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* I2C: QUP7 */
	i2c@89c000 {
		#address-cells = <1>;
		#size-cells = <0>;

		pinctrl-0 = <&sm_gpio_98 &sm_gpio_99>;
		pinctrl-1 = <&sm_gpio_98_suspend &sm_gpio_99_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
		tcs3490@72 {
			compatible = "ams,tcs3490";
			reg = <0x72>;
			interrupt-parent = <&tlmm>;
			interrupts = <37 0x0>;
			rgbcir_gpio_vdd-supply = <&rgbcir_vreg>;
			rgbcir_vio-supply = <&pm8150l_l1>;
			pinctrl-names = "rgbcir_irq_active", "rgbcir_irq_suspend";
			pinctrl-0 = <&sm_gpio_37>;
			pinctrl-1 = <&sm_gpio_37_suspend>;
			ams,rgbcir-vdd-supply = <0>;
			ams,rgbcir-gpio-vdd = <1>;
			ams,rgbcir-vio-supply = <1>;
		};
	};

	/* I2C: QUP10 */
	i2c@a88000 {
		pinctrl-0 = <&sm_gpio_9 &sm_gpio_10>;
		pinctrl-1 = <&sm_gpio_9_suspend &sm_gpio_10_suspend>;
		qcom,clk-freq-out = <400000>;
		status = "okay";
	};

	/* UART: QUP12 */
	qcom,qup_uart@a90000 {
		pinctrl-0 = <&sm_gpio_85 &sm_gpio_86>;
		pinctrl-1 = <&sm_gpio_85 &sm_gpio_86>;
		status = "okay";
	};

	/* UART: QUP13 */
	qcom,qup_uart@c8c000 {
		status = "okay";
	};

	qcom,wdt@17c10000 {
		qcom,bark-time = <12000>;
	};

	last_logs@ffbc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};

	restart-reason {
		compatible = "qcom,restart-reason";

		none {
			cmd = "none";
			reg-val = <0x00 0x00000000>;
		};

		unknown {
			cmd = "unknown";
			reg-val = <0x01 0x77665501>;
		};

		recovery {
			cmd = "recovery";
			reg-val = <0x02 0x77665500>;
			reboot-cmd;
		};

		bootloader {
			cmd = "bootloader";
			reg-val = <0x03 0x77665500>;
			reboot-cmd;
		};

		rtc {
			cmd = "rtc";
			reg-val = <0x04 0x77665500>;
			reboot-cmd;
		};

		dm-verity-device-corrupted {
			cmd = "dm-verity device corrupted";
			reg-val = <0x05 0x77665508>;
			reboot-cmd;
		};

		dm-verity-enforcing {
			cmd = "dm-verity enforcing";
			reg-val = <0x06 0x77665509>;
			reboot-cmd;
		};

		keys-clear {
			cmd = "keys clear";
			reg-val = <0x07 0x7766550A>;
			reboot-cmd;
		};

		panic {
			cmd = "panic";
			reg-val = <0x40 0xC0DEDEAD>;
		};

		oem-N {
			cmd = "oem-4E";
			reg-val = <0x61 0x6F656D4E>;
			reboot-cmd;
		};

		oem-S {
			cmd = "oem-53";
			reg-val = <0x60 0x6F656D53>;
			reboot-cmd;
		};

		oem-P {
			cmd = "oem-50";
			reg-val = <0x51 0x6F656D50>;
			reboot-cmd;
		};
	};

	somc_water_detection: somc_water_detection {
		compatible = "somc,water-detection";
		dvdt_enable_gpio = <&tlmm 131 0>;
		dvdt_wrt_det_and = <&tlmm 87 0>;
		dvdt_wrt_det_or = <&tlmm 119 0>;
	};
};

&usb0 {
	dwc3@a600000 {
		maximum-speed = "super-speed";
	};
};

&wil6210 {
	status = "disabled";
};

&qupv3_se9_i2c {
	status = "disabled";
};

/* Regulator config */
&pm8150_l13 {
	qcom,regulator-type = <0>;      /* LDO */
	qcom,init-enable = <1>;
	qcom,init-ldo-mode = <1>;
	qcom,init-pin-ctrl-enable = <0>;
	qcom,init-pin-ctrl-mode = <0>;
	regulator-min-microvolt = <2700000>;
	regulator-max-microvolt = <2700000>;
	qcom,init-voltage = <2700000>;
	status = "okay";
};

&pm8150_l14 {
	qcom,regulator-type = <0>;      /* LDO */
	qcom,init-enable = <1>;
	qcom,init-ldo-mode = <1>;
	qcom,init-pin-ctrl-enable = <0>;
	qcom,init-pin-ctrl-mode = <0>;
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <1800000>;
	qcom,init-voltage = <1800000>;
	status = "okay";
};

&pm8150l_bob {
	regulator-min-microvolt = <3350000>;
	qcom,init-voltage = <3350000>;
	status = "okay";
};

&pm8150l_bob_ao {
	regulator-min-microvolt = <3350000>;
	qcom,init-voltage = <3350000>;
	status = "okay";
};

&pm8150_gpios {
	qcom,gpios-disallowed = <2 5 8>;

	interrupts = <0x0 0xc0 0 IRQ_TYPE_NONE>,
			<0x0 0xc2 0 IRQ_TYPE_NONE>,
			<0x0 0xc3 0 IRQ_TYPE_NONE>,
			<0x0 0xc5 0 IRQ_TYPE_NONE>,
			<0x0 0xc6 0 IRQ_TYPE_NONE>,
			<0x0 0xc8 0 IRQ_TYPE_NONE>,
			<0x0 0xc9 0 IRQ_TYPE_NONE>;
	interrupt-names = "pm8150_gpio1", "pm8150_gpio3",
			"pm8150_gpio4", "pm8150_gpio6", "pm8150_gpio7",
			"pm8150_gpio9", "pm8150_gpio10";

	/* GPIO_1: VOL_DOWN_N */
	pm8150_gpio_1: pm8150_gpio_1 {
		pins = "gpio1";
		function = "normal";
		input-enable;
		bias-pull-up;
		power-source = <0>;
	};

	/* GPIO_3: NC */
	pm8150_gpio_3: pm8150_gpio_3 {
		pins = "gpio3";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_3 */
	};

	/* GPIO_4: NC */
	pm8150_gpio_4: pm8150_gpio_4 {
		pins = "gpio4";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_4 */
	};

	/* GPIO_6: NC */
	pm8150_gpio_6: pm8150_gpio_6 {
		pins = "gpio6";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_6 */
	};

	/* GPIO_7: NC */
	pm8150_gpio_7: pm8150_gpio_7 {
		pins = "gpio7";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_7 */
	};

	/* GPIO_9: NC */
	pm8150_gpio_9: pm8150_gpio_9 {
		pins = "gpio9";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_9 */
	};

	/* GPIO_10: NC */
	pm8150_gpio_10: pm8150_gpio_10 {
		pins = "gpio10";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_10 */
	};
};

&pm8150b_gpios {

	/delete-property/ qcom,gpios-disallowed;
	interrupts = <0x2 0xc0 0 IRQ_TYPE_NONE>,
			<0x2 0xc1 0 IRQ_TYPE_NONE>,
			<0x2 0xc2 0 IRQ_TYPE_NONE>,
			<0x2 0xc3 0 IRQ_TYPE_NONE>,
			<0x2 0xc4 0 IRQ_TYPE_NONE>,
			<0x2 0xc5 0 IRQ_TYPE_NONE>,
			<0x2 0xc6 0 IRQ_TYPE_NONE>,
			<0x2 0xc7 0 IRQ_TYPE_NONE>,
			<0x2 0xc8 0 IRQ_TYPE_NONE>,
			<0x2 0xc9 0 IRQ_TYPE_NONE>,
			<0x2 0xca 0 IRQ_TYPE_NONE>,
			<0x2 0xcb 0 IRQ_TYPE_NONE>;
	interrupt-names = "pm8150b_gpio1", "pm8150b_gpio2",
			"pm8150b_gpio3", "pm8150b_gpio4",
			"pm8150b_gpio5", "pm8150b_gpio6",
			"pm8150b_gpio7", "pm8150b_gpio8",
			"pm8150b_gpio9", "pm8150b_gpio10",
			"pm8150b_gpio11", "pm8150b_gpio12";

	/* GPIO_1: SNAPSHOT_N */
	pm8150b_gpio_1: pm8150b_gpio_1 {
		pins = "gpio1";
		function = "normal";
		input-enable;
		bias-pull-up;
		power-source = <0>;
	};

	/* GPIO_2: FOCUS_N */
	pm8150b_gpio_2: pm8150b_gpio_2 {
		pins = "gpio2";
		function = "normal";
		input-enable;
		bias-pull-up;
		power-source = <0>;
	};

	/* GPIO_3: NC */
	pm8150b_gpio_3: pm8150b_gpio_3 {
		pins = "gpio3";
		function = "normal";
		bias-high-impedance; /* DISABLE GPIO_3 */
	};

	/* GPIO_4: NC */
	pm8150b_gpio_4: pm8150b_gpio_4 {
		pins = "gpio4";
