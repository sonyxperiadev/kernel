menuconfig ARC_SOC_HSDK
	bool "ARC HS Development Kit SOC"
	depends on ISA_ARCV2
	select ARC_HAS_ACCL_REGS
	select ARC_IRQ_NO_AUTOSAVE
	select CLK_HSDK
	select RESET_CONTROLLER
	select RESET_HSDK
