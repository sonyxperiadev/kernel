&pm8226_gpios {
	/* GPIO_1: PMIC_AUDIO_REF_CLLK*/
	/* Follow QCT */
	gpio@c000 {
		/* XO_PMIC_CDC_MCLK enable for tapan codec */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;	/* CMOS logic */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO*/
		qcom,vin-sel = <3>;		/* QPNP_PIN_VIN3 */
		qcom,out-strength = <3>;/* QPNP_PIN_OUT_STRENGTH_HIGH */
		qcom,src-sel = <2>;		/* QPNP_PIN_SEL_FUNC_1 */
		qcom,master-en = <1>;	/* Enable GPIO */
	};

	/* GPIO_2: NFC_EN*/
	gpio@c100 {
		qcom,mode = <1>; /* QPNP_PIN_MODE_DIG_OUT */
		qcom,output-type = <0>; /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,invert = <0>; /* QPNP_PIN_INVERT_DISABLE */
		qcom,pull = <5>; /* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <3>; /* QPNP_PIN_VIN3 = VREG_L6 */
		qcom,out-strength = <1>; /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,src-sel = <0>; /* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>; /* enable this gpio */
	};

	/* GPIO_3: NFC_CLK_REQ*/
	gpio@c200 {
		qcom,mode = <0>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,src-sel = <2>;		/* QPNP_PIN_SEL_FUNC_1 */
		qcom,master-en = <1>;
	};

	/* GPIO_4: BATT_UIM_ALARM*/
	/* Follow QCT */
	gpio@c300 {
	};

	/* GPIO_5: FLASH_STROBE_TRIG*/
	/* Follow QCT */
	gpio@c400 {
	};

	/* GPIO_6: NC*/
	gpio@c500 {
		qcom,mode = <0>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <4>;		/* QPNP_PIN_PULL_DN */
		qcom,master-en = <1>;
	};

	/* GPIO_7: TX_GTR_THRESH*/
	/* Follow QCT */
	gpio@c600 {
	};

	/* GPIO_8: NC*/
	gpio@c700 {
		qcom,mode = <0>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <4>;		/* QPNP_PIN_PULL_DN */
		qcom,master-en = <1>;
	};
};

&pm8226_mpps {
	/* MPP_1: VREF_MSM_PX_MPP1 */
	/* Follow QCT */
	mpp@a000 {
	};

	/* MPP_2: LED3_SINK */
	mpp@a100 {
	};

	/* MPP_3: VREF_LDO_MPP3 */
	/* Follow QCT */
	mpp@a200 {
	};

	/* MPP_4: LED1_SINK */
	/* Follow QCT */
	mpp@a300 {
	};

	/* MPP_5: PA_THERMEL_0 */
	/* Follow QCT */
	mpp@a400 {
		/* PA_THERM0 config */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <0>; /* AMUX 5 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};

	/* MPP_6: LED2_SINK */
	/* Follow QCT */
	mpp@a500 {
	};

	/* MPP_7: MSM_THERMEL */
	mpp@a600 {
		/* MSM_THERM config */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <2>; /* AMUX 7 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};

	/* MPP_8: PA_THERMEL_1 */
	/* Follow QCT */
	mpp@a700 {
		/* PA_THERM1 config */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <3>; /* AMUX 8 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};
};

