/ {
	model = "SoMC Leo ROW";
	compatible = "somc,leo-row", "qcom,msm8974";
	qcom,board-id = <8 0>;
	qcom,msm-id = <194 0x10000>,
		      <209 0x10000>,
		      <210 0x10000>,
		      <212 0x10000>,
		      <213 0x10000>,
		      <215 0x10000>,
		      <216 0x10000>,
		      <218 0x10000>;
	somc,space-no = "1280-4852",	/* Gina */
			"1280-4876",	/* Rita */
			"1280-4918",	/* Gina-APAC */
			"1282-0073";	/* Katyusha */
};
