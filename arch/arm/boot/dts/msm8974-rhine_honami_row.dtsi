				qcom,scale-function = <8>;
			};
		};

		qcom,vadc@3400 {
			chan@30 {
				qcom,hw-settle-time = <15>;
			};

			chan@8 {
				qcom,hw-settle-time = <15>;
			};

			chan@6 {
				qcom,hw-settle-time = <15>;
			};

			chan@b4 {
				qcom,scale-function = <4>;
			};
		};
	};
};

