&sfpb_spinlock {
	status = "disable";
};

&ldrex_spinlock {
	status = "ok";
};

&usb_otg {
	qcom,hsusb-otg-pnoc-errata-fix;
};

&usb3 {
	qcom,usbin-vadc = <&pm8941_vadc>;
	dwc3@f9200000 {
		snps,ssphy-clear-auto-suspend-on-disconnect;
		snps,usb3-u1u2-disable;
	};
};

&gdsc_venus {
	qcom,skip-logic-collapse;
	qcom,retain-periph;
	qcom,retain-mem;
};

&gdsc_mdss {
	qcom,skip-logic-collapse;
	qcom,retain-periph;
	qcom,retain-mem;
};

&gdsc_oxili_gx {
