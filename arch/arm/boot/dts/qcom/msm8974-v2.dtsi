				<63 512 4264000 1600000>;
			qcom,bus-configs = <0x30fcfff>;
		};

		qcom,msm-bus-client@2 {
			qcom,msm-bus,name = "venc-ocmem";
			qcom,msm-bus,num-cases = <10>;
			qcom,msm-bus,num-paths = <1>;
			qcom,msm-bus,vectors-KBps =
				<68 604 0 0>,
				<68 604 138000 1034000>, /* VGA 30 fps */
				<68 604 414000 1034000>, /* VGA 60 fps */
				<68 604 414000 1034000>, /* 720p 30 fps */
