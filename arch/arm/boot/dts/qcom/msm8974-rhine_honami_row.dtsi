#include "dsi-panel-honami.dtsi"
#include "msm8974-rhine_common.dtsi"
#include "msm8974-camera-sensor-honami_row.dtsi"

&soc {
	/* BLSP2 */
	i2c_2: i2c@f9924000 {
		synaptics_clearpad@2c {
			chip_id = <0x36>;
			num_sensor_rx = <28>;
			num_sensor_tx = <15>;
			charger_supported = <0>;
			pen_supported = <0>;
			glove_supported = <0>;
			cover_supported = <0>;
			touch_pressure_enabled = <1>;
			touch_size_enabled = <0>;
			touch_orientation_enabled = <0>;
			preset_x_max = <1079>;
			preset_y_max = <1919>;
			preset_n_bytes_per_object = <8>;
			delay_after_hw_reset = <200>;
			wakeup_gesture_supported = <0>;
			wakeup_gesture_timeout = <0>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		interrupt-parent = <&msmgpio>;
		interrupts = <9 0>;

		camera_snapshot {
			label = "camera_snapshot";
			gpios = <&pm8941_gpios 3 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		camera_focus {
			label = "camera_focus";
			gpios = <&pm8941_gpios 4 0x1>;
			linux,input-type = <1>;
			linux,code = <0x210>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};
};

&pm8941_chg {
	qcom,ibatmax-ma = <1500>;
	qcom,ibatterm-ma = <150>;
	qcom,ibatsafe-ma = <1500>;
	qcom,maxinput-dc-ma = <1800>;
	qcom,maxinput-usb-ma = <1500>;
	qcom,ibatmax-warm-ma = <900>;
	qcom,ibatmax-cool-ma = <900>;
	qcom,thermal-mitigation = <1500 1500 1100 900 700 500 300 200 100 0>;
};

&pm8941_bms {
	qcom,r-sense-uohm = <10005>;
	qcom,fcc-resolution = <13>;
	qcom,chg-term-ua = <150000>;
};

&pm8941_gpios {
	/* GPIO_3: SNAPSHOT */
	gpio@c200 {
		qcom,src-sel = <0>;		/* GPIO */
		qcom,mode = <0>;		/* In */
		qcom,vin-sel = <2>;		/* S3 */
		qcom,pull = <0>;		/* PU */
		qcom,master-en = <1>;		/* Enable */
		status = "ok";
	};

	/* GPIO_4: FOCUS */
	gpio@c300 {
		qcom,src-sel = <0>;		/* GPIO */
		qcom,mode = <0>;		/* In */
		qcom,vin-sel = <2>;		/* S3 */
		qcom,pull = <0>;		/* PU */
		qcom,master-en = <1>;		/* Enable */
		status = "ok";
	};

	/* GPIO_27: FLASH_LED_NOW */
	gpio@da00 {
		qcom,src-sel = <2>;		/* Alternate function 1 */
		qcom,mode = <0>;		/* In */
		qcom,vin-sel = <2>;		/* S3 */
		qcom,pull = <4>;		/* PD */
		qcom,master-en = <1>;		/* Enable */
		status = "ok";
	};

};

&spmi_bus {
	qcom,pm8941@0 {
		iadc@3600 {
			qcom,rsense = <10005000>;
		};
	};

	qcom,pm8941@1 {
		qcom,leds@d000 {
			qcom,rgb_0 {
				qcom,pwm-max-value = <304>;
			};

			qcom,rgb_1 {
				qcom,pwm-max-value = <511>;
			};

			qcom,rgb_2 {
				qcom,pwm-max-value = <368>;
			};
		};

		qcom,leds@d300 {
			status = "okay";
		};
	};
};

&pm8941_flash0 {
	qcom,clamp-curr = <0>;
};

&pm8941_torch {
	qcom,clamp-curr = <0>;
};

/* Regulator config */
&pm8941_l23 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <2800000>;
	qcom,init-enable = <0>;
	qcom,init-voltage = <2800000>;
	status = "ok";
};

&slim_msm {
	taiko_codec {
		qcom,cdc-micbias2-headset-only;
	};
};

&mdss_fb0 {
	qcom,memory-reservation-size = <0x1000000>; /* size 16MB */
};

&mdss_mdp {
	qcom,mdss-ib-factor = <2 1>;		/* 2 times  */
	somc,dric-gpio = <&msmgpio 26 0>;
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_renesas_default>;
	qcom,platform-enable-gpio = <&msmgpio 46 1>;
};
