	qcom,sensor-information {
		/* msm_therm */
		sensor_information17: qcom,sensor-information-17 {
			qcom,scaling-factor = <10>;
		};

		/* emmc_therm */
		sensor_information18: qcom,sensor-information-18 {
			qcom,scaling-factor = <10>;
		};

		/* quiet_therm */
