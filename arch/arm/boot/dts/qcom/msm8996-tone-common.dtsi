/ {
	aliases {
		i2c3 = &i2c_3;
		i2c11 = &i2c_11;
	};

	reserved-memory {
		debug_region: debug_region@a7e00000 {
			compatible = "removed-dma-pool", "qcom,debug_memory";
			no-map;
			reg = <0 0xa7e00000 0 0x100000>;
			label = "debug_mem";
		};

		pstore_reserve_mem: pstore_reserve_mem_region_region@a7f00000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0xa7f00000 0 0x00100000>;
		};
	};
};

&soc {
	pinctrl@01010000 {
		disabled-pins = <0 1 2 3>;
	};

	fpc1145 {
		status = "ok";
		compatible = "fpc,fpc1020", "fpc1145";
		interrupt-parent = <&tlmm>;
		interrupts = <132 0x0>;
		fpc,gpio_rst    = <&tlmm 21 0x0>;
		fpc,gpio_irq    = <&tlmm 132 0x0>;
		vcc_spi-supply  = <&pm8994_s4>;
		vdd_ana-supply  = <&pm8994_l18>;
		vdd_io-supply   = <&pm8994_l18>;

		pinctrl-names = "fpc1145_reset_reset",
				"fpc1145_reset_active",
				"fpc1145_irq_active";

		pinctrl-0 = <&msm_gpio_21>;
		pinctrl-1 = <&msm_gpio_21_output_high>;
		pinctrl-2 = <&msm_gpio_132>;
	};

	pcie1: qcom,pcie@00608000 {
		status = "disabled";
	};

	pcie2: qcom,pcie@00610000 {
		status = "disabled";
	};

	/* SPI : BLSP1 */
	spi@7575000 {
		pinctrl-0 = <&msm_gpio_0 &msm_gpio_1 &msm_gpio_2 &msm_gpio_3>;
		pinctrl-1 = <&msm_gpio_0 &msm_gpio_1 &msm_gpio_2 &msm_gpio_3>;
		status = "disabled";
	};

	/* UART: BLSP2 */
	uart@07570000 {
		compatible = "qcom,msm-hsuart-v14";
		reg = <0x07570000 0x1000>,
		      <0x7544000 0x2b000>;
		reg-names = "core_mem", "bam_mem";
		interrupt-names = "core_irq", "bam_irq";
		#address-cells = <0>;
		interrupt-parent = <&blsp1_uart2>;
		interrupts = <0 1>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0xffffffff>;
		interrupt-map = <0 &intc 0 0 108 0
				1 &intc 0 0 238 0>;
		/delete-property/ qcom,inject-rx-on-wakeup;
		/delete-property/ qcom,rx-char-to-inject;
		qcom,bam-tx-ep-pipe-index = <2>;
		qcom,bam-rx-ep-pipe-index = <3>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&msm_gpio_41 &msm_gpio_42 &msm_gpio_43 &msm_gpio_44_active>;
		pinctrl-1 = <&msm_gpio_41 &msm_gpio_42 &msm_gpio_43 &msm_gpio_44_suspend>;
		status = "okay";
	};

	bluesleep {
		compatible = "qcom,bluesleep";
		bt_host_wake = <&tlmm 82 0x00>; /* BT_HOST_WAKE */
		bt_ext_wake = <&tlmm 81 0x00>; /* BT_DEV_WAKE */
		interrupt-parent = <&tlmm>;
		interrupts = <82 0>;
		interrupt-names = "host_wake";
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&msm_gpio_82_active &msm_gpio_81>;
		pinctrl-1 = <&msm_gpio_82_suspend &msm_gpio_81>;
	};

	bcm43xx {
		compatible = "bcm,bcm43xx";
		bcm,reg-on-gpio = <&tlmm 83 0x00>; /* BT_REG_ON */
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&msm_gpio_83>;
		pinctrl-1 = <&msm_gpio_83>;
	};

	/* UART: BLSP3 : NC */
	qcom,cnss {
		status = "disabled";
	};

	/* I2C : BLSP3 */
	i2c_3: i2c@7577000 { /* BLSP1 QUP2 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x7577000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 97 0>;
		dmas = <&dma_blsp1 16 32 0x20000020 0x20>,
			<&dma_blsp1 17 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <86>;
		qcom,clk-freq-out = <355000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
			<&clock_gcc clk_gcc_blsp1_qup3_i2c_apps_clk>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&msm_gpio_47 &msm_gpio_48>;
		pinctrl-1 = <&msm_gpio_47 &msm_gpio_48>;
		status = "disabled";
	};

	/* I2C : BLSP7 */
	i2c@75b5000 {
		qcom,clk-freq-out = <355000>;
		pinctrl-0 = <&msm_gpio_55 &msm_gpio_56>;
		pinctrl-1 = <&msm_gpio_55 &msm_gpio_56>;
		status = "okay";
	};

	/* UART: BLSP8 */
	serial@075b0000 {
		pinctrl-names = "uart_active";
		pinctrl-0 = <&msm_gpio_4 &msm_gpio_5>;
		status = "okay";
	};

	/* I2C : BLSP8 */
	i2c@75b6000 { /* BLSP2 QUP1 */
		qcom,clk-freq-out = <355000>;
		pinctrl-0 = <&msm_gpio_6 &msm_gpio_7>;
		pinctrl-1 = <&msm_gpio_6 &msm_gpio_7>;
		status = "okay";

		/delete-node/ nq@28;

		nfc@28 {
			compatible = "nxp,pn547";
			reg = <0x28>;
			interrupt-parent = <&tlmm>;
			interrupts = <9 0x1>;
			nxp,irq_gpio = <&tlmm 9 0x00>;
			nxp,dwld_en = <&tlmm 12 0x00>;
			nxp,ven = <&pm8994_gpios 7 0x01>;
			qcom,clk-src = "BBCLK2";
			qcom,clk-gpio = <&pm8994_gpios 10 0x00>;
			clocks = <&clock_gcc clk_bb_clk2_pin>;
			clock-names = "nfc_clk";
			pinctrl-names = "pn547-active", "pn547-inactive";
			pinctrl-0 = <&msm_gpio_9 &msm_gpio_12>;
			pinctrl-1 = <&msm_gpio_9 &msm_gpio_12>;
		};
	};

	/* I2C : BLSP11 */
	i2c_11: i2c@75b9000 { /* BLSP2 QUP4 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x75b9000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 105 0>;
		dmas = <&dma_blsp2 20 64 0x20000020 0x20>,
			<&dma_blsp2 21 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <84>;
		qcom,clk-freq-out = <355000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
			 <&clock_gcc clk_gcc_blsp2_qup5_i2c_apps_clk>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&msm_gpio_60 &msm_gpio_61>;
		pinctrl-1 = <&msm_gpio_60 &msm_gpio_61>;
		status = "disabled";
	};

	/* SPI : BLSP12 */
	/* I2C : BLSP12 */
	i2c@75ba000 {
		qcom,clk-freq-out = <355000>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&msm_gpio_87_act &msm_gpio_88_act>;
		pinctrl-1 = <&msm_gpio_87_sus &msm_gpio_88_sus>;
		status = "okay";

		/delete-node/ synaptics@20;

		synaptics_clearpad@2c {
			compatible = "synaptics,clearpad";
			reg = <0x2c>;
			interrupt-parent = <&tlmm>;
			interrupts = <125 0x2>;
			synaptics,irq_gpio = <&tlmm 125 0x00>;
			post_probe_start = <1>;
			synaptics,firmware_name = "touch_module_id_0x%02x.img";
			flash_on_post_probe = <0>;
			flip_config = <0>;
			watchdog_enabled = <1>;
			watchdog_delay_ms = <3000>;
			charger_supported = <0>;
			pen_supported = <0>;
			glove_supported = <0>;
			cover_supported = <1>;
			touch_pressure_enabled = <1>;
			touch_size_enabled = <0>;
			touch_orientation_enabled = <0>;
			preset_x_max = <1079>;
			preset_y_max = <1919>;
			preset_n_fingers = <10>;
			wakeup_gesture_supported = <0>;
			wakeup_gesture_lpm_disabled = <0>;
			wakeup_gesture_timeout = <0>;
			wakeup_gesture {
				double_tap {
					gesture_code = <0x0003>;
					event_00 {
						type = <2>; /* LOG */
						message = "=== DOUBLE TAP ===";
					};
					event_01 {
						type = <1>; /* KEY */
						code = <143>; /* WAKEUP */
						down = <1>;
					};
					event_02 {
						type = <1>; /* KEY */
						code = <143>; /* WAKEUP */
						down = <0>;
					};
					event_03 {
						type = <99>; /* END */
					};
				};
			};
			/* chip settings */
			clearpad_default {
				flash_default_timeout_ms = <20000>;
				calibrate_on_fwflash = <0>;
				calibration_supported = <0>;
				hwreset_delay_for_powerup_ms = <220>;
				interrupt_default_wait_ms = <1000>;
				charger_only_delay_ms = <200>;
			};
			S3330 {
				flash_default_timeout_ms = <20000>;
				calibrate_on_fwflash = <0>;
				calibration_supported = <0>;
				hwreset_delay_for_powerup_ms = <220>;
				interrupt_default_wait_ms = <1000>;
				charger_only_delay_ms = <200>;
			};
			S332U {
				flash_default_timeout_ms = <20000>;
				calibrate_on_fwflash = <0>;
				calibration_supported = <0>;
				hwreset_delay_for_powerup_ms = <220>;
				interrupt_default_wait_ms = <1000>;
				charger_only_delay_ms = <200>;
			};
		};
	};

	bcmdhd_wlan {
		compatible = "android,bcmdhd_wlan";
		gpios = <&tlmm 84 0x00>, /* WL_REG_ON */
			<&tlmm 79 0x00>; /* WL_HOST_WAKE */
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&msm_gpio_84 &msm_gpio_79>;
		pinctrl-1 = <&msm_gpio_84 &msm_gpio_79>;
		wlan-pci-number = <0>;

		qcom,msm-bus,name = "msm-bcmdhd";
		qcom,msm-bus,num-cases = <3>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps =
			<45 512 0 0>, /* None vote */
			<45 512 500 800>, /* Low vote */
			<45 512 500 5448000>; /* High Vote */
	};

	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = <&msm_gpio_4
				&msm_gpio_5 &msm_gpio_6 &msm_gpio_7 &msm_gpio_8 &msm_gpio_9
				&msm_gpio_10 &msm_gpio_11 &msm_gpio_12 &msm_gpio_13
				&msm_gpio_14 &msm_gpio_15 &msm_gpio_16 &msm_gpio_17
				&msm_gpio_18 &msm_gpio_19 &msm_gpio_20 &msm_gpio_21
				&msm_gpio_22 &msm_gpio_23 &msm_gpio_24 &msm_gpio_25
				&msm_gpio_26 &msm_gpio_27 &msm_gpio_28 &msm_gpio_29
				&msm_gpio_30 &msm_gpio_31 &msm_gpio_32 &msm_gpio_33
				&msm_gpio_34 &msm_gpio_38 &msm_gpio_39 &msm_gpio_40
				&msm_gpio_41 &msm_gpio_42 &msm_gpio_43 &msm_gpio_44_suspend
				&msm_gpio_45 &msm_gpio_46 &msm_gpio_47 &msm_gpio_48
				&msm_gpio_49 &msm_gpio_50 &msm_gpio_51 &msm_gpio_52
				&msm_gpio_55 &msm_gpio_56 &msm_gpio_57 &msm_gpio_58
				&msm_gpio_59 &msm_gpio_60 &msm_gpio_61 &msm_gpio_62
				&msm_gpio_63 &msm_gpio_69 &msm_gpio_73 &msm_gpio_74
				&msm_gpio_75 &msm_gpio_76 &msm_gpio_77 &msm_gpio_78
				&msm_gpio_79 &msm_gpio_81 &msm_gpio_82_suspend &msm_gpio_83
				&msm_gpio_84 &msm_gpio_85 &msm_gpio_86 &msm_gpio_87
				&msm_gpio_88 &msm_gpio_89 &msm_gpio_90 &msm_gpio_91
				&msm_gpio_92 &msm_gpio_93 &msm_gpio_94 &msm_gpio_95
				&msm_gpio_96 &msm_gpio_108 &msm_gpio_112 &msm_gpio_116
				&msm_gpio_120 &msm_gpio_121 &msm_gpio_122 &msm_gpio_123
				&msm_gpio_124 &msm_gpio_125 &msm_gpio_126 &msm_gpio_130
				&msm_gpio_131 &msm_gpio_132 &msm_gpio_141 &msm_gpio_142
				&msm_gpio_143>;

		/* If product common default setting is needed,
		   fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		   fill pinctrl-2 value in <variant>.dtsi */
