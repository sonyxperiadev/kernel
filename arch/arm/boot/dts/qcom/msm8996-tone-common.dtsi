};

&pmx_mdss {
	mdss_dsi_active: mdss_dsi_active {
		mux {
			pins = "gpio8";
			function = "gpio";
		};

		config {
			pins = "gpio8";
			drive-strength = <2>; /* 2 mA */
			bias-disable = <0>; /* no pull */
		};
	};
	mdss_dsi_suspend: mdss_dsi_suspend {
		mux {
			pins = "gpio8";
			function = "gpio";
		};

		config {
			pins = "gpio8";
			drive-strength = <2>; /* 2 mA */
			/delete-property/ bias-pull-down;
			bias-disable = <0>; /* no pull */
			output-low;
		};
	};
};

&mdss_dsi0 {
	vddio-supply = <&pm8994_s4>;
	touch-avdd-supply = <&pm8994_l22>;
	qcom,display-id = "primary";
	qcom,dsi-pref-prim-pan = <&dsi_default_panel>;
	pinctrl-names = "mdss_default", "mdss_sleep", "mdss_touch_active", "mdss_touch_suspend";
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend>;
	pinctrl-2 = <&mdss_touch_active>;
	pinctrl-3 = <&mdss_touch_suspend>;
	qcom,platform-te-gpio = <&tlmm 10 0>;
	qcom,platform-reset-gpio = <&tlmm 8 0>;
	qcom,platform-vddio-gpio = <&tlmm 51 0>;
	qcom,platform-touch-vddio-gpio = <&tlmm 50 0>;
	qcom,platform-touch-reset-gpio = <&tlmm 89 0>;
	qcom,platform-touch-int-gpio = <&tlmm 125 0>;
	/delete-property/ qcom,platform-bklight-en-gpio;
