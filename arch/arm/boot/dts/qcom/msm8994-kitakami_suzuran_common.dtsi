/ {
	memory {

		fb_mem: fb_region@0 {
			linux,reserve-contiguous-region;
			linux,reserve-region;
			reg = <0 0x40000000 0 0x0730000>;
			label = "fb_mem";
		};
	};
};

&soc {
	/* I2C : BLSP6 */
	i2c@f9928000 {
		synaptics_clearpad@2c {
			num_sensor_rx = <26>;
			num_sensor_tx = <16>;
			preset_x_max = <719>;
			preset_y_max = <1279>;
		};
	};

	/delete-node/ qcom,qseecom@6500000;
	qcom,qseecom@e700000 {
		compatible = "qcom,qseecom";
		reg = <0xe700000 0x1900000>;
		reg-names = "secapp-region";
		qcom,disk-encrypt-pipe-pair = <2>;
		qcom,file-encrypt-pipe-pair = <0>;
		qcom,support-multiple-ce-hw-instance;
		qcom,hlos-num-ce-hw-instances = <2>;
		qcom,hlos-ce-hw-instance = <1 2>;
		qcom,qsee-ce-hw-instance = <0>;
		qcom,msm-bus,name = "qseecom-noc";
		qcom,msm-bus,num-cases = <4>;
		qcom,msm-bus,num-paths = <1>;
		qcom,support-fde;
		qcom,support-pfe;
		qcom,no-clock-support;
		qcom,msm-bus,vectors-KBps =
				<55 512 0 0>,
				<55 512 0 0>,
				<55 512 120000 1200000>,
				<55 512 393600 3936000>;
		clock-names = "core_clk", "ufs_core_clk_src", "ufs_core_clk",
			      "ufs_bus_clk", "ufs_iface_clk";
		clocks = <&clock_rpm clk_qseecom_ce1_clk>,
                         <&clock_gcc clk_ufs_axi_clk_src>,
                         <&clock_gcc clk_gcc_ufs_axi_clk>,
                         <&clock_gcc clk_gcc_sys_noc_ufs_axi_clk>,
			 <&clock_gcc clk_gcc_ufs_ahb_clk>;
		qcom,ce-opp-freq = <171430000>;
	};
};

&pmi8994_gpios {
	/* GPIO_3: NC (TUNER_ANT_SW_EN) */
	gpio@c200 {
		qcom,master-en = <0>;		/* Disable */
		status = "ok";
	};

	/* GPIO_4: NC (TUNER_ANT_SW1) */
	gpio@c300 {
		qcom,master-en = <0>;		/* Disable */
		status = "ok";
	};

	/* GPIO_5: NC */
	gpio@c400 {
		qcom,master-en = <0>;		/* Disable */
		status = "ok";
	};
};

&pm8994_l14 {
	regulator-min-microvolt = <2000000>;
	regulator-max-microvolt = <2000000>;
	qcom,init-voltage = <2000000>;
};

&pm8994_l19 {
	status = "disabled";
};

&labibb {
	qpnp-labibb-mode = "lcd";
	qcom,qpnp-ibb-discharge-resistor = <32>;
};

&lab_regulator {
	qcom,qpnp-lab-limit-maximum-current = <200>;
};

&ibb_regulator {
	qcom,qpnp-ibb-limit-maximum-current = <800>;
};

&kitakami_panel_power_supply {
	qcom,panel-supply-entry@0 {
		qcom,supply-min-voltage = <2000000>;
		qcom,supply-max-voltage = <2000000>;
	};
};

&sdhc_3 {
	vdd-io-supply = <&pm8994_s4>;
	qcom,vdd-io-voltage-level = <1800000 1800000>;
	qcom,vdd-io-current-level = <200 22000>;
	pinctrl-names = "active", "sleep";
	pinctrl-0 = <&sdc3_clk_on &sdc3_cmd_on &sdc3_data_on>;
	pinctrl-1 = <&sdc3_clk_off &sdc3_cmd_off &sdc3_data_off>;
	qcom,clk-rates = <400000 20000000 25000000 50000000 100000000 200000000>;
	qcom,bus-speed-mode = "SDR12", "SDR25", "SDR50", "DDR50", "SDR104";
	somc,use-for-wifi;
	status = "ok";
};

&spmi_bus {
	qcom,pmi8994@3 {
		qcom,leds@d000 {
			status = "okay";
			qcom,rgb_sync = <1>;

			qcom,rgb_0 {
				somc,pwm-channel = <2>;
				qcom,pwm-max-value = <90>;
				linux,name = "led:rgb_red";
				linux,default-trigger = "none";
			};
			qcom,rgb_1 {
				somc,pwm-channel = <1>;
				qcom,pwm-max-value = <150>;
				linux,name = "led:rgb_green";
				linux,default-trigger = "none";
			};
			qcom,rgb_2 {
				somc,pwm-channel = <0>;
				qcom,pwm-max-value = <200>;
				linux,name = "led:rgb_blue";
				linux,default-trigger = "none";
			};
		};
		qcom,leds@d800 {
			qcom,ilim-ma = <660>;
			qcom,fs-curr-ua = <25000>;
			somc,bl-scale-enabled;
			somc,init-br = <800>;
			somc-s1,br-power-save = <131>;
			qcom,led-strings-list = [00 01];
		};
	};
};

&tlmm_pinmux {
	/* GPIO_35: NC */
	msm_gpio_35 {
		qcom,pins = <&gp 35>;
		qcom,num-grp-pins = <1>;
		label = "msm_gpio_35";
		qcom,pin-func = <0>;
		msm_gpio_35_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	/* GPIO_36: NC */
	msm_gpio_36 {
		qcom,pins = <&gp 36>;
		qcom,num-grp-pins = <1>;
		label = "msm_gpio_36";
		qcom,pin-func = <0>;
		msm_gpio_36_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	/* GPIO_37: NC */
	msm_gpio_37 {
		qcom,pins = <&gp 37>;
		qcom,num-grp-pins = <1>;
		label = "msm_gpio_37";
		qcom,pin-func = <0>;
		msm_gpio_37_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	/* GPIO_41: NC */
	msm_gpio_41 {
		qcom,pin-func = <0>;
		msm_gpio_41_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
			/delete-property/ bias-pull-up;
			/delete-property/ input-enable;
		};
	};

	/* GPIO_77: NC */
	msm_gpio_77 {
		qcom,pin-func = <0>;
		msm_gpio_77_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
			/delete-property/ input-enable;
		};
	};

	/* GPIO_122: NC */
	msm_gpio_122 {
		qcom,pin-func = <0>;
		msm_gpio_122_def: default {
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	pmx_sdc2_clk {
		sdc2_clk_on: clk_on {
			drive-strength = <12>;
		};
	};
};

&somc_pinctrl {
	pinctrl-1 = <&msm_gpio_35_def &msm_gpio_36_def &msm_gpio_37_def &msm_gpio_49_def &msm_gpio_50_def
		     &msm_gpio_51_def &msm_gpio_117_def &msm_gpio_118_def &msm_gpio_125_def &msm_gpio_130_def
		     &msm_gpio_132_def &msm_gpio_133_def &msm_gpio_142_def &msm_gpio_143_def>;
};

&hsphy0 {
       qcom,hsphy-init = <0x00FBDFA7>;
       qcom,hsphy-init-host = <0x00FBC7A7>;
};

&mdss_dsi {
	hw-config = "single_dsi";
};

&mdss_dsi0 {
	/delete-property/ qcom,platform-enable-gpio;
};

/{
	kitakami_suzuran_batterydata: qcom,battery-data {
		#include "batterydata-kitakami-suzuran-send.dtsi"
	};
};

&pmi8994_fg {
	qcom,battery-data = <&kitakami_suzuran_batterydata>;
	qcom,fg-iterm-ma = <140>;
	qcom,fg-chg-iterm-ma = <140>;
};

&pmi8994_charger {
	qcom,fastchg-current-ma = <2100>;
	somc,fastchg-warm-current-ma = <700>;
	somc,fastchg-cool-current-ma = <700>;
	qcom,iterm-ma = <150>;
	qcom,precharging-timeout-mins = <24>;
	qcom,charging-timeout-mins = <768>;
	somc,thermal-engine-fastchg-current = <2100 1880 1600 1100 900 700 500 400 300 300 300 300 300 0 0>;
	somc,thermal-mitigation-usb-9v = <1500 1400 1300 1000 1000 1000 900 900 700 500 400 300 300 0 0>;
	somc,thermal-mitigation-usb-5v = <1500 1500 1500 1500 1500 1500 1500 1500 1200 900 700 500 300 0 0>;
	somc,limit-usb-5v-level = <12>;
	somc,charge-full-design = <2781000>;
};

&qsecom_mem {
	reg = <0 0 0 0x1900000>;
};

&removed_regions {
	reg = <0 0x06300000 0 0x9D00000>;
};

&pcie1 {
	status = "disabled";
};
