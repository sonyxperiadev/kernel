			qcom,iommu-ctx-sids = <0x2c03>;
			label = "adsp_sec_bitstream";
		};

		venus_fw: qcom,iommu-ctx@1e23000 {
			compatible = "qcom,msm-smmu-v2-ctx";
			reg = <0x1e23000 0x1000>;
			qcom,secure-context;
