&soc {
        ramoops {
                status = "ok";
	};

	/* I2C : BLSP8 */
	i2c@f9964000 {
		synaptics_clearpad@2c {
			compatible = "synaptics,clearpad";
			reg = <0x2c>;
			interrupt-parent = <&msmgpio>;
			interrupts = <86 0x2>;
			touch_vdd-supply = <&pm8941_l22>;
			touch_vio-supply = <&pm8941_s3>;
			synaptics,irq_gpio = <&msmgpio 86 0x00>;
			synaptics,reset_gpio = <&msmgpio 85 0>;
			reset_l2h = <0>;
			chip_id = <0x38>;
			num_sensor_rx = <26>;
			num_sensor_tx = <16>;
			charger_supported = <0>;
			pen_supported = <0>;
			glove_supported = <1>;
			cover_supported = <1>;
			touch_pressure_enabled = <1>;
			touch_size_enabled = <0>;
			touch_orientation_enabled = <0>;
			preset_x_max = <719>;
			preset_y_max = <1279>;
			preset_n_fingers = <10>;
			delay_after_hw_reset = <200>;
			wakeup_gesture_supported = <1>;
			wakeup_gesture_lpm_disabled = <1>;
			wakeup_gesture_timeout = <2000>;
			wakeup_gesture {
				double_tap {
					gesture_code = <0x0003>;
					event_00 {
						type = <2>; /* LOG */
						message = "=== DOUBLE TAP ===";
					};
					event_01 {
						type = <1>; /* KEY */
						code = <116>; /* KEY_POWER */
						down = <1>;
					};
					event_02 {
						type = <1>; /* KEY */
						code = <116>; /* KEY_POWER */
						down = <0>;
					};
					event_03 {
						type = <99>; /* END */
					};
				};
			};
		};
	};

	gpio_keys {
		camera_snapshot {
			label = "camera_snapshot";
			gpios = <&pm8941_gpios 3 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		camera_focus {
			label = "camera_focus";
			gpios = <&pm8941_gpios 4 0x1>;
			linux,input-type = <1>;
			linux,code = <0x210>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		sim_det {
			label = "sim-detection";
			gpios = <&msmgpio 9 0x0>;
			linux,input-type = <5>;
			linux,code = <7>;
			gpio-key,wakeup;
			debounce-interval = <10>;
		};
	};

	bu520x1nvx {
		compatible = "rohm,bu520x1nvx";

		acc_cover {
			label = "lid";
			gpios = <&pm8941_gpios 36 0x1>;
			lid-pin = <1>;
			debounce-interval = <50>;
		};
	};
};

&camera_mem {
	reg = <0x78714000 0x6400000>;
};

&pm8941_chg {
	qcom,ibatmax-ma = <1800>;
	qcom,ibatterm-ma = <130>;
	qcom,ibatsafe-ma = <2100>;
	qcom,maxinput-dc-ma = <1800>;
	qcom,maxinput-usb-ma = <1500>;
	qcom,maxinput-usb-mv = <13000>;
	qcom,thermal-mitigation = <1800 1600 1100 900 700 500 300 200 100 0>;
	qcom,ibatmax-warm-ma = <750>;
	qcom,ibatmax-cool-ma = <750>;
	qcom,step-thresh-soc = <60>;
	qcom,ibatmax-ma-over-step = <1300>;
	qcom,ibatmax-ma-under-step = <1800>;
};

&pm8941_bms {
	qcom,chg-term-ua = <130000>;
	qcom,fcc-resolution = <14>;
	qcom,battery-vendor-name = "TDK", "SEND", "SANYO-PANASONIC", "LG", "5TH";
	qcom,battery-vendor-adc-min = <1450 1330 1230 1110 960>;
	qcom,battery-vendor-adc-max = <1550 1430 1330 1210 1060>;
	qcom,battery-vendor-select = <0 1 0 0 0>;
};

&pm8941_lsid0 {
	pm8941_iadc: iadc@3600 {
		qcom,rsense = <10000000>;
	};
};

&pm8941_gpios {
	/* GPIO_27: FLASH_LED_NOW */
	gpio@da00 {
		qcom,src-sel = <2>;		/* Alternate function 1 */
		qcom,mode = <0>;		/* In */
		qcom,vin-sel = <2>;		/* S3 */
		qcom,pull = <4>;		/* PD */
		qcom,master-en = <1>;		/* Enable */
		status = "ok";
	};

	/* GPIO_36: ACC_COVER_OPEN */
	gpio@e300 {
		qcom,src-sel = <0>;		/* GPIO */
		qcom,mode = <0>;		/* In */
		qcom,vin-sel = <2>;		/* S3 */
		qcom,pull = <0>;		/* PU */
		qcom,master-en = <1>;		/* Enable */
		status = "ok";
	};
};

&spmi_bus {
	qcom,pm8941@1 {
		qcom,vib@c000 {
			qcom,qpnp-vib-vtg-level-mV = <3100>;
		};

		qcom,leds@d000 {
			status = "okay";
			qcom,rgb_sync = <1>;

			qcom,rgb_0 {
				qcom,pwm-max-value = <255>;
			};

			qcom,rgb_1 {
				qcom,pwm-max-value = <511>;
			};

			qcom,rgb_2 {
				qcom,pwm-max-value = <511>;
			};
		};

		qcom,leds@d300 {
			status = "okay";
		};

		qcom,leds@d800 {
			status = "okay";
			qcom,wled_0 {
				label = "wled";
				linux,name = "wled:backlight";
				linux,default-trigger = "bkl-trigger";
				qcom,cs-out-en;
				qcom,cabc-en = <0>;
				qcom,op-fdbck = <0>;
				qcom,default-state = "on";
				qcom,max-current = <20>;
				qcom,init-br = <1023>;
				somc-s1,br-power-save = <163>;
				qcom,ctrl-delay-us = <0>;
				qcom,boost-curr-lim = <3>;
				qcom,cp-sel = <0>;
				qcom,switch-freq = <11>;
				qcom,ovp-val = <2>;
				qcom,num-strings = <2>;
				qcom,id = <0>;
				qcom,full-scale-seg@0 {
					threshold = <1023>;
					curr = <5>;
					coef = <4>;
				};
				qcom,full-scale-seg@1 {
					threshold = <2047>;
					curr = <10>;
					coef = <2>;
				};
			};
		};
	};
};

&mdss_mdp {
	somc,dric-gpio = <&msmgpio 26 0>;
	somc,mul-channel-scaling = <3>;
};

&mdss_dsi0 {
	qcom,platform-enable-gpio = <&msmgpio 78 0>;
};
