					<10 806400000>,
					<11 883200000>,
					<12 960000000>,
					<13 1036800000>,
					<14 1113600000>,
					<15 1190400000>,
					<16 1267200000>,
					<17 1344000000>,
					<18 1363200000>,
					<19 1420800000>,
					<20 1459200000>,
					<21 1497600000>,
					<22 1536000000>,
					<23 1574400000>,
					<24 1612800000>,
					<25 1632000000>,
					<26 1651200000>,
					<27 1689600000>,
					<28 1708800000>;
		qcom,speed-bin-fuse-sel = <1 34 3 0>;
		qcom,fuse-remap-base-row = <1000>;
		qcom,fuse-remap-source =
					<0 29 1 0>, /* speed bit[0]	*/
