&tsens {
	qcom,sensors = <10>;
	qcom,slope = <2911 2789 2906 2763 2922 2867 2833 2838 2840 2852>;
	qcom,sensor-id = <0 1 2 3 5 6 7 8 9 10>;
};

&bam_dmux {
	qcom,no-cpu-affinity;
};

&mdss_dsi0 {
	qcom,platform-regulator-settings = [03 08 07 00 20 07 01];
