/dts-v1/;

#include "msm8974-v2.2.dtsi"
#include "msm8974-mtp.dtsi"
#include "msm8974-rhine_honami_row.dtsi"

/ {
	model = "SoMC Honami ROW";
	compatible = "somc,honami-row", "qcom,msm8974";
	qcom,board-id = <8 0>;

	qcom,msm-id = <126 0x20001>,
		      <185 0x20001>,
		      <186 0x20001>,
		      <126 0x20002>,
		      <185 0x20002>,
		      <186 0x20002>;
};

&pm8941_chg {
	qcom,bpd-detection = "bpd_thm";
};
