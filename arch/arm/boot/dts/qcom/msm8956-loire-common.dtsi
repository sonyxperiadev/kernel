	mdss_dsi: qcom,mdss_dsi@0 {
		vdda-supply = <&pm8950_l23>;

		mdss_dsi0: qcom,mdss_dsi_ctrl0@1a94000 {
			vdd-supply = <&pm8950_l23>;
			vddio-supply = <&pm8950_l6>;
			touch-avdd-supply = <&pm8950_l22>;
		};

		mdss_dsi1: qcom,mdss_dsi_ctrl1@1a96000 {
			vdd-supply = <&pm8950_l23>;
			vddio-supply = <&pm8950_l6>;
			touch-avdd-supply = <&pm8950_l22>;
		};
	};

