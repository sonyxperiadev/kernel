			qcom,sensor-mode = <1>;
			qcom,cci-master = <0>;
			sony,i2c_addr = <0x20>;
			sony,eeprom_addr = <0xA0>;
			sony,eeprom_type = <0>;
			sony,eeprom_max_len = <2048>;
			sony,gpio_af = <0>;
			sony,subdev_code = <0x3007>;

			sony,camera_modules {
				module_names = "GENERIC", "SOI20BS1";
				default_module_name = "SOI20BS1";

				GENERIC {
					mount_angle = <90>;
					sensor_rotation = <0>;
					sensor_facing = <0>;
					pixel_number_w = <5248>;
					pixel_number_h = <3936>;
					diagonal_len = "7.87";
					unit_cell_size = "1.20";
					min_f_number = "2.00";
					max_f_number = "2.00";
					has_focus_actuator = <1>;
					has_3a = <0>;
					pll_num = <18>;
